{
  "components/themevale/sidebar/side-all-categories": "<div class=\"sidebarBlock\" id=\"sideAllCategories\">\n    <h5 class=\"sidebarBlock-heading\">Kategorien</h5>\n    {{#if categories}}\n    <ul class=\"all-categories-list\">\n        {{#each categories}}\n        <li class=\"navPages-item\">\n            {{#if children}}\n            <a class=\"navPages-action has-subMenu\" href=\"{{url}}\">\n                <span>{{name}}</span>\n            </a>\n            <i class=\"icon-dropdown\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-right\" /></svg></i>\n            <ul class=\"dropdown-category-list navPages-level-2\">\n                {{#each children}}\n                    <li class=\"navPage-subMenu-item\">\n                        {{#if children}}\n                            <a class=\"navPage-subMenu-action navPages-action has-subMenu\" href=\"{{url}}\">\n                                <span>{{name}}</span>\n                            </a>\n                            <i class=\"icon-dropdown\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-right\" /></svg></i>\n                            <ul class=\"dropdown-category-list navPages-level-3\">\n                                {{#each children}}\n                                <li class=\"navPage-childList-item\">\n                                    {{#if children}}\n                                    <a class=\"navPage-childList-action navPages-action has-subMenu\" href=\"{{url}}\"><span>{{name}}</span></a>\n                                    <i class=\"icon-dropdown\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-right\" /></svg></i>\n                                    <ul class=\"dropdown-category-list navPages-level-4\">\n                                        {{#each children}}\n                                        <li class=\"navPage-childList-item\">\n                                            {{#if children}}\n                                                <a class=\"navPage-childList-action navPages-action has-subMenu\" href=\"{{url}}\"><span>{{name}}</span></a>\n                                                <i class=\"icon-dropdown\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-right\" /></svg></i>\n                                                <ul class=\"dropdown-category-list navPages-level-5\">\n                                                    {{#each children}}\n                                                     <li class=\"navPage-childList-item\">\n                                                         <a class=\"navPage-childList-action navPages-action \" href=\"{{url}}\"><span>{{name}}</span></a>\n                                                     </li>\n                                                     {{/each}}\n                                                </ul>\n                                            {{else}}\n                                            <a class=\"navPage-childList-action navPages-action \" href=\"{{url}}\"><span>{{name}}</span></a>\n                                            {{/if}}\n                                        </li>\n                                        {{/each}}\n                                    </ul>\n                                    {{else}}\n                                    <a class=\"navPage-childList-action navPages-action \" href=\"{{url}}\"><span>{{name}}</span></a>\n                                    {{/if}}\n                                </li>\n                                {{/each}}\n                            </ul>\n                        {{else}}\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\"><span>{{name}}</span></a>\n                        {{/if}}\n                    </li>\n                {{/each}}\n            </ul>\n            {{else}}\n            <a class=\"navPages-action\" href=\"{{url}}\"><span>{{name}}</span></a>\n            {{/if}}\n        </li>\n        {{/each}}\n    </ul>\n    {{/if}}\n</div>\n"
