{
  "components/themevale/sidebar/side-all-categories": "<div class=\"sidebarBlock\" id=\"sideAllCategories\">\r\n    <h5 class=\"sidebarBlock-heading\">Kategorien</h5>\r\n    {{#if categories}}\r\n    <ul class=\"all-categories-list\">\r\n        {{#each categories}}\r\n        <li class=\"navPages-item\">\r\n            {{#if children}}\r\n            <a class=\"navPages-action has-subMenu\" href=\"{{url}}\">\r\n                <span>{{name}}</span>\r\n            </a>\r\n            <i class=\"icon-dropdown\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-right\" /></svg></i>\r\n            <ul class=\"dropdown-category-list navPages-level-2\">\r\n                {{#each children}}\r\n                    <li class=\"navPage-subMenu-item\">\r\n                        {{#if children}}\r\n                            <a class=\"navPage-subMenu-action navPages-action has-subMenu\" href=\"{{url}}\">\r\n                                <span>{{name}}</span>\r\n                            </a>\r\n                            <i class=\"icon-dropdown\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-right\" /></svg></i>\r\n                            <ul class=\"dropdown-category-list navPages-level-3\">\r\n                                {{#each children}}\r\n                                <li class=\"navPage-childList-item\">\r\n                                    {{#if children}}\r\n                                    <a class=\"navPage-childList-action navPages-action has-subMenu\" href=\"{{url}}\"><span>{{name}}</span></a>\r\n                                    <i class=\"icon-dropdown\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-right\" /></svg></i>\r\n                                    <ul class=\"dropdown-category-list navPages-level-4\">\r\n                                        {{#each children}}\r\n                                        <li class=\"navPage-childList-item\">\r\n                                            {{#if