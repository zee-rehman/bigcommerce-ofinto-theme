{
  "components/themevale/product/ask-an-expert-content": "<div class=\"modal-content\">\r\n   <div class=\"modal-header\">\r\n      <h2 class=\"modal-header-title\">Hast Du eine Frage?</h2>\r\n   </div>\r\n   <div class=\"logo\">\r\n      <a href=\"/\">\r\n         <img class=\"lazyload\" data-src=\"/images/stencil/491x116/logo_green_1610551752__35233.original.png\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\" width=\"100\">\r\n      </a>\r\n   </div>\r\n   <div class=\"modal-body\">\r\n      <div id=\"contact_results\"></div>\r\n      <div class=\"form-inner\">\r\n         <div class=\"title-wrapper\">\r\n             <h4 class=\"title\">Sende Deine Frage direkt an den ofinto Experten</h4>\r\n             <div class=\"description\">Wir antworten Dir innerhalb von 24 Stunden.</div>\r\n             <div class=\"questions\">Bitte fülle den folgenden Fragebogen aus:</div>\r\n         </div>\r\n         \r\n         <form class=\"form\" id=\"contact_form\">\r\n            <fieldset class=\"form-fieldset\">\r\n               <div class=\"form-field\">\r\n                  <label class=\"form-label\">Name, Vorname</label>\r\n                  <input required=\"true\" type=\"text\" class=\"form-input\" name=\"customer_name\" value=\"{{customer.name}}\">\r\n               </div>\r\n               <div class=\"form-field\">\r\n                  <label class=\"form-label\">Email-Adresse</label>\r\n                  <input required=\"true\" type=\"text\" class=\"form-input\" name=\"customer_email\" value=\"{{customer.email}}\">\r\n               </div>\r\n               <div class=\"form-field\">\r\n                  <label class=\"form-label\">Telefon-Nummer</label>\r\n                  <input required=\"true\" type=\"text\" class=\"form-input\" name=\"customer_phone\" value=\"{{customer.phone}}\">\r\n               </div>\r\n               <div class=\"form-field form-field-1 form-field-radio\">\r\n                  <label class=\"form-label\">Thema:</label>\r\n                  <input class=\"form-radio\" id=\"pricing-radio\" type=\"radio\" name=\"type_package\" checked value=\"Pricing\">\r\n                  <label class=\"form-label\" for=\"pricing-radio\">Preise</label>\r\n                  <input class=\"form-radio\" id=\"answers-radio\" type=\"radio\" name=\"type_package\" value=\"Answers\">\r\n                  <label class=\"form-label\" for=\"answers-radio\">Produkt</label>\r\n                  <input class=\"form-radio\" id=\"both-radio\" type=\"radio\" name=\"type_package\" value=\"Pricing & Answers\">\r\n                  <label class=\"form-label\" for=\"both-radio\">Anderes</label>\r\n               </div>\r\n               <div class=\"form-field\">\r\n                  <label class=\"form-label\" for=\"comment-area\">Wie können wir Dir helfen?</label>\r\n                  <textarea name=\"message\" cols=\"30\" rows=\"6\" class=\"form-input\" id=\"comment-area\" required=\"true\"></textarea>\r\n               </div>\r\n               <div class=\"form-field\">\r\n                  <label class=\"form-label label-last\">Wie sollen wir Dich kontaktieren?</label>\r\n                  <input class=\"form-radio\" id=\"phone-radio\" type=\"radio\" name=\"type_contact\" checked value=\"Phone\">\r\n                  <label class=\"form-label\" for=\"phone-radio\">Telefon</label>\r\n                  <input class=\"form-radio\" id=\"email-radio\" type=\"radio\" name=\"type_contact\" value=\"Email\">\r\n                  <label class=\"form-label\" for=\"email-radio\">Email</label>\r\n                  <input class=\"form-radio\" id=\"both-radio-2\" type=\"radio\" name=\"type_contact\" value=\"Phone & Email\">\r\n                  <label class=\"form-label\" for=\"both-radio-2\">Beides</label>\r\n               </div>\r\n               <div class=\"form-actions\">\r\n                 <button type=\"button\" class=\"button button--primary button--large\" id=\"ask-an-expert-button\">Anfrage absenden</button>\r\n               </div>\r\n            </fieldset>\r\n         </form>\r\n      </div>\r\n   </div>\r\n</div>\r\n"
}
