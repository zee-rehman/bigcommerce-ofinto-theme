  "components/common/json-mega-menu": "<!-- Mega Menu Container -->\r\n<nav class=\"mega-menu\">\r\n\t<ul class=\"nav-json-items nav-items\">\r\n      {{!-- Mega Menu Items will be dynamically added here by jQuery --}}\r\n    </ul>\r\n</nav>",
  "components/common/navigation-2": "<nav class=\"navUser\">\n    \n    <div class=\"navUser-section navUser-section--help\">\n        {{!-- Help header nav and dropdown appear dynamically from json customize file --}}\n    </div>\n    \n    {{> components/common/currency-selector}}\n\n    <div class=\"navUser-section navUser-section--alt\">\n        <div class=\"navUser-item navUser-item--quichSearch\">\n            <a class=\"navUser-action navUser-action--search\" href=\"javascript:void(0)\">\n                <svg class=\"icon icon-search\"><use xlink:href=\"#icon-search\"></use></svg>\n            </a>\n            <div class=\"themevaleSearch\">\n                {{> components/common/quick-search}}\n            </div>\n        </div>\n        \n        <div class=\"navUser-item navUser-item--account\">\n            <a class=\"navUser-action navUser-action--account\" href=\"javascript:void(0)\">\n                {{#if customer}}\n                    <span class=\"text\">\n                        <svg class=\"icon icon-user-header\"><use xlink:href=\"#icon-user\"></use></svg>\n                    </span>\n                {{else}}\n                    <span class=\"text\">\n                        <svg class=\"icon icon-user-header\"><use xlink:href=\"#icon-user\"></use></svg>\n                    </span>\n                {{/if}}\n            </a>\n\n            <div class=\"dropdown-menu account-dropdown\" id=\"accountDropdown\">\n                {{> components/themevale/login-form}}\n            </div>\n\n        </div>\n        \n        <div class=\"navUser-item navUser-item--cart\">\n            <a\n                class=\"navUser-action\"\n                data-cart-preview\n                data-themevale-dropdown\n                data-options=\"align:right\"\n                href=\"{{urls.cart}}\">\n                <svg class=\"icon\"><use xlink:href=\"#icon-cart\"></use></svg>\n                <span class=\"countPill cart-quantity\"></span>\n            </a>\n        </div>        \n    </div>\n</nav>",
  "components/themevale/header/header-mobile": "<div class=\"themevale_header\">\n    <div class=\"header-mobile-item text-center\">\n        <div class=\"items\">\n            <div class=\"logo-wrapper\">\n                {{#if page_type '===' 'default'}}\n                    <h1 class=\"header-logo\">\n                        {{> components/common/store-logo}}\n                    </h1>\n                {{else}}\n                    <div class=\"header-logo\">\n                        {{> components/common/store-logo}}\n                    </div>\n                {{/if}}\n            </div>\n        </div>\n    </div>\n    <div class=\"header-mobile-item text-right\">\n        <div class=\"items item--searchMobile\">\n            <a class=\"navUser-action searchMobile\" href=\"javascript:void(0)\" data-search=\"quickSearch\" aria-controls=\"quickSearch\" aria-expanded=\"false\">\n                <svg class=\"icon\"><use xlink:href=\"#icon-search\"></use></svg>\n                <span class=\"icon close\">&times;</span>\n            </a>\n        </div>\n        <div class=\"items item--account\">\n            <a class=\"navUser-action accountMobile\" href=\"javascript:void(0)\">\n                <div class=\"navUser-icon\">\n                    <svg class=\"icon\"><use xlink:href=\"#icon-user\"/></svg>\n                </div>\n            </a>\n        </div>\n        <div class=\"items item--cart themevale_cart\">\n            <a class=\"navUser-action cartMobile\"\n                data-cart-preview\n                data-options=\"align:right\"\n                href=\"{{urls.cart}}\">\n                <div class=\"navUser-icon navUser-item-cartIcon cart-icon\">\n                    <svg class=\"icon\"><use xlink:href=\"#icon-cart\"></use></svg>\n                    <span class=\"countPill cart-quantity\"></span>\n                </div>\n            </a>\n        </div>\n        <div class=\"items item--hamburger\">\n            <a class=\"mobileMenu-toggle\" href=\"javascript:void(0)\" data-mobile-menu-toggle=\"menu\">\n                <span class=\"mobileMenu-toggleIcon\">Toggle menu</span>\n            </a>\n        </div>\n    </div>\n</div>\n\n{{!--\n<script>\n$(document).ready(function() {\n\t$(document).on('click touchstart', '#menu-mobile .nav-item', function(event) {\n\t\tevent.preventDefault();\n\t\t$('#menu-mobile .nav-item').removeClass('jsonNav-active');\n\t\t$('body').removeClass('jsonNav-active');\n\t\t$('#menu-mobile .nav-item .mega-menu-content').hide();\n\t\t$(this).closest('#menu-mobile .nav-item').addClass('jsonNav-active');\n\t\t$('body').addClass('jsonNav-active');\n\t\t$(this).siblings('#menu-mobile .nav-item .mega-menu-content').show();\n\t});\n});\n$(document).on('click touchstart', '#menu-mobile h4.column-hd.mob-hd', function() {\n\tsetTimeout (function(){\n\t\t$('.nav-item').removeClass('jsonNav-active');\n\t\t$('body').removeClass('jsonNav-active');\n\t},100 );\n});\n$(document).on('click touchstart', '#menu-mobile .themevale_close .close', function() {\n\t$('.nav-item').removeClass('jsonNav-active');\n\t$('body').removeClass('jsonNav-active');\n});\n</script>\n--}}",
  "components/common/currency-selector": "<div class=\"navUser-section navUser-section--currency\">\n    <!-- use shop specific file -->\n    {{> shop_specific/currency-selector }}\n</div>\n\n",
  "components/common/quick-search": "<form class=\"form\" action=\"{{urls.search}}\">\n    <fieldset class=\"form-fieldset\">\n        <div class=\"form-field\">\n            <label class=\"is-srOnly\" for=\"search_query\">{{lang \"search.quick_search.input_label\"}}</label>\n            <input type=\"text\" class=\"form-input\" data-search-quick name=\"search_query\" id=\"search_query\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\">\n            <button type=\"submit\"><svg class=\"icon\"><use xlink:href=\"#icon-search\"></use></svg></button>\n        </div>\n    </fieldset>\n</form>\n<div class=\"themevale-dropdown dropdown--quickSearch\" id=\"quickSearch\" aria-hidden=\"true\" tabindex=\"-1\" data-prevent-quick-search-close>\n\t<div class=\"header-search-wrap\">\n        {{#if theme_settings.enable_search_trending}}\n        <div class=\"header-search__trending\">\n        \t{{#if theme_settings.search-trending-title '!==' \"\"}}\n        \t\t<h3 class=\"search-form__product-title\">{{theme_settings.search-trending-title}}</h3>\n        \t{{/if}}\n        \t<ul class=\"list-item\">\n        \t\t{{#if theme_settings.search-trending-text-1 '!==' \"\"}}\n        \t\t\t<li>\n        \t\t\t\t<a href=\"search.php?search_query={{theme_settings.search-trending-text-1}}&section=content\">\n    \t\t    \t\t\t<svg class=\"icon\"><use xlink:href=\"#icon-search\"></use></svg>\n    \t\t    \t\t\t<span>{{theme_settings.search-trending-text-1}}</span>\n        \t\t\t\t</a>\n        \t\t\t</li>\n        \t\t{{/if}}\n        \t\t{{#if theme_settings.search-trending-text-2 '!==' \"\"}}\n        \t\t\t<li>\n        \t\t\t\t<a href=\"search.php?search_query={{theme_settings.search-trending-text-2}}&section=content\">\n    \t    \t\t\t\t<svg class=\"icon\"><use xlink:href=\"#icon-search\"></use></svg>\n    \t    \t\t\t\t<span>{{theme_settings.search-trending-text-2}}</span>\n        \t\t\t\t</a>\n        \t\t\t</li>\n        \t\t{{/if}}\n        \t\t{{#if theme_settings.search-trending-text-3 '!==' \"\"}}\n        \t\t\t<li>\n        \t\t\t\t<a href=\"search.php?search_query={{theme_settings.search-trending-text-3}}&section=content\">\n        \t\t\t\t\t<svg class=\"icon\"><use xlink:href=\"#icon-search\"></use></svg>\n        \t\t\t\t\t<span>{{theme_settings.search-trending-text-3}}</span>\n        \t\t\t\t</a>\n        \t\t\t</li>\n        \t\t{{/if}}\n        \t\t{{#if theme_settings.search-trending-text-4 '!==' \"\"}}\n        \t\t\t<li>\n        \t\t\t\t<a href=\"search.php?search_query={{theme_settings.search-trending-text-4}}&section=content\">\n        \t\t\t\t\t<svg class=\"icon\"><use xlink:href=\"#icon-search\"></use></svg>\n        \t\t\t\t\t<span>{{theme_settings.search-trending-text-4}}</span>\n        \t\t\t\t</a>\n        \t\t\t</li>\n        \t\t{{/if}}\n        \t\t{{#if theme_settings.search-trending-text-5 '!==' \"\"}}\n        \t\t\t<li>\n        \t\t\t\t<a href=\"search.php?search_query={{theme_settings.search-trending-text-5}}&section=content\">\n        \t\t\t\t\t<svg class=\"icon\"><use xlink:href=\"#icon-search\"></use></svg>\n        \t\t\t\t\t<span>{{theme_settings.search-trending-text-5}}</span>\n        \t\t\t\t</a>\n        \t\t\t</li>\n        \t\t{{/if}}\n        \t\t{{#if theme_settings.search-trending-text-6 '!==' \"\"}}\n        \t\t\t<li>\n        \t\t\t\t<a href=\"search.php?search_query={{theme_settings.search-trending-text-6}}&section=content\">\n        \t\t\t\t\t<svg class=\"icon\"><use xlink:href=\"#icon-search\"></use></svg>\n        \t\t\t\t\t<span>{{theme_settings.search-trending-text-6}}</span>\n        \t\t\t\t</a>\n        \t\t\t</li>\n        \t\t{{/if}}\n        \t</ul>\n        </div>\n        {{/if}}\n        \n        {{#if theme_settings.enable_search_product_block}}\n        <div id=\"search_product_block\" class=\"search_product_block\">\n        \t{{#if theme_settings.search-product-block-title '!==' \"\"}}\n        \t\t<h3 class=\"search-form__product-title\">{{theme_settings.search-product-block-title}}</h3>\n        \t{{/if}}\n            <ul class=\"productGrid{{#if theme_settings.show_brand_product_gallery}} show-brand{{/if}}\">\n            </ul>\n        </div>\n        {{/if}}\n    </div>\n    <section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\n</div>\n",
  "components/themevale/login-form": "<nav class=\"login-dropdown\" id=\"login-dropdown\">\n    {{#if customer}}\n        <p class=\"navPage-subMenu-title\">\n            <span class=\"text\">{{lang 'common.account'}}</span> \n            <span class=\"icon\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></span>\n        </p>\n        <ul class=\"navPages-list navPages-list--user\">\n            <li class=\"navPages-item\">\n                <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.orders.all}}\"><span class=\"text\">{{lang 'account.nav.orders'}}</span></a>\n            </li>\n            {{#if settings.returns_enabled}}\n                <li class=\"navPages-item\">\n                    <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.returns}}\"><span class=\"text\">{{lang 'account.nav.returns'}}</span></a>\n                </li>\n            {{/if}}\n            <li class=\"navPages-item\">\n                <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.addresses}}\"><span class=\"text\">{{lang 'account.nav.addresses'}}</span></a>\n            </li>\n            <li class=\"navPages-item\">\n                <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.wishlists.all}}\"><span class=\"text\">{{lang 'account.mobile_nav.wishlists'}}</span></a>\n            </li>\n            <li class=\"navPages-item\">\n                <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.recent_items}}\"><span class=\"text\">{{lang 'account.nav.recently_viewed'}}</span></a>\n            </li>\n            <li class=\"navPages-item\">\n                <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.details}}\"><span class=\"text\">{{lang 'account.nav.settings'}}</span></a>\n            </li>\n            <li class=\"navPages-item\">\n                <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.auth.logout}}\"><span class=\"text\">{{lang 'common.logout'}}</span></a>\n            </li>\n        </ul>\n        {{else}}\n            <p class=\"navPage-subMenu-title\">\n                <span class=\"text\">Kunden Login</span> \n                <span class=\"icon\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></span>\n            </p>\n            <div class=\"popup-login-form\">\n                <div class=\"login-content\">\n                    <form class=\"login-form form\" action=\"{{settings.secure_base_url}}/login.php?action=check_login\" method=\"post\">\n                        {{inject 'useValidEmail' (lang 'forms.validate.common.email_address')}}\n                        {{inject 'enterPass' (lang 'forms.validate.common.password')}}\n                        <div class=\"form-field\">\n\n                            <label class=\"form-label\" for=\"login_email\">{{lang 'login.field_email'}} <small>{{lang 'common.required2'}}</small></label>\n                            <input class=\"form-input\" name=\"login_email\" id=\"login_email\" type=\"email\" placeholder=\"{{lang 'login.field_email'}}\">\n                        </div>\n                        <div class=\"form-field\">\n                            <label class=\"form-label\" for=\"login_pass\">{{lang 'login.field_password'}} <small>{{lang 'common.required2'}}</small></label>\n                            <input class=\"form-input\" id=\"login_pass\" type=\"password\" name=\"login_pass\" placeholder=\"{{lang 'login.field_password'}}\">\n                        </div>\n                        <div class=\"form-actions\">\n                            <input type=\"submit\" class=\"button button--border button--large\" value=\"{{lang 'login.button' }}\">\n                            <a class=\"forgot-password\" href=\"{{urls.auth.forgot_password}}\">{{lang 'login.forgot_password' }}</a>\n                        </div>\n                    </form>\n                </div>\n                <div class=\"new-customer\">\n                    {{#if settings.account_creation_enabled}}\n                        <a class=\"button button--primary button--large\" href=\"{{urls.auth.create_account}}\">{{lang 'create_account.button' }}</a>\n                    {{/if}}\n                </div>\n            </div>\n        {{/if}}\n</nav>\n",
  "shop_specific/currency-selector": "<div class=\"navUser-item\">\n    <a class=\"navUser-action navUser-action--currencySelector has-dropdown\" href=\"#\" data-dropdown=\"currencySelection\" aria-controls=\"currencySelection\" aria-expanded=\"false\">\n        <span class=\"label-only\">\n            <span class=\"image-flag\"><img src=\"/product_images/uploaded_images/flag-ch.svg\" alt=\"ofinto Schweiz\"></span>\n            <span class=\"text-flag\">CH</span>\n        </span>\n        <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use href=\"#icon-chevron-down\"></use></svg></i></a>\n    <ul class=\"dropdown-menu\" id=\"currencySelection\" data-dropdown-content aria-hidden=\"true\" tabindex=\"-1\">\n        <li class=\"dropdown-menu-item\">\n            <a href=\"#Weglot-de\">\n                {{#if is_active}}\n                <span class=\"image-flag\"><img src=\"/product_images/uploaded_images/flag-ch.svg\" alt=\"Schweiz (de)\"></span><span class=\"text-flag\">Schweiz (de)</span>\n                {{else}}\n                <span class=\"image-flag\"><img src=\"/product_images/uploaded_images/flag-ch.svg\" alt=\"Schweiz (de)\"></span><span class=\"text-flag\">Schweiz (de)</span>\n                {{/if}}\n            </a>\n        </li>\n        <li class=\"dropdown-menu-item\">\n            <a href=\"#Weglot-fr\">\n                {{#if is_active}}\n                <span class=\"image-flag\"><img src=\"/product_images/uploaded_images/flag-ch.svg\" alt=\"Suisse (fr)\"></span><span class=\"text-flag\">Suisse (fr)</span>\n                {{else}}\n                <span class=\"image-flag\"><img src=\"/product_images/uploaded_images/flag-ch.svg\" alt=\"Suisse (fr)\"></span><span class=\"text-flag\">Suisse (fr)</span>\n                {{/if}}\n            </a>\n        </li>\n        <li class=\"dropdown-menu-item\">\n            <a href=\"#Weglot-en\">\n                {{#if is_active}}\n                <span class=\"image-flag\"><img src=\"/product_images/uploaded_images/flag-ch.svg\" alt=\"Switzerland (en)\"></span><span class=\"text-flag\">Switzerland (en)</span>\n                {{else}}\n                <span class=\"image-flag\"><img src=\"/product_images/uploaded_images/flag-ch.svg\" alt=\"Switzerland (en)\"></span><span class=\"text-flag\">Switzerland (en)</span>\n                {{/if}}\n            </a>\n        </li>\n        <li class=\"dropdown-menu-item\">\n            <a href=\"https://ofinto.de\">\n                {{#if is_active}}\n                <span class=\"image-flag\"><img src=\"/product_images/uploaded_images/flag-ch.svg\" alt=\"Deutschland\"></span><span class=\"text-flag\">Deutschland</span>\n                {{else}}\n                <span class=\"image-flag\"><img src=\"/product_images/uploaded_images/flag-ch.svg\" alt=\"Deutschland\"></span><span class=\"text-flag\">Deutschland</span>\n                {{/if}}\n            </a>\n        </li>\n        <li class=\"dropdown-menu-item\">\n            <a href=\"https://ofinto.at\">\n                {{#if is_active}}\n                <span class=\"image-flag\"><img src=\"/product_images/uploaded_images/flag-ch.svg\" alt=\"Österreich\"></span><span class=\"text-flag\">Österreich</span>\n                {{else}}\n                <span class=\"image-flag\"><img src=\"/product_images/uploaded_images/flag-ch.svg\" alt=\"Österreich\"></span><span class=\"text-flag\">Österreich</span>\n                {{/if}}\n            </a>\n        </li>\n        <li class=\"dropdown-menu-item\">\n            <a href=\"https://ofinto.fr\">\n                {{#if is_active}}\n                <span class=\"image-flag\"><img src=\"/product_images/uploaded_images/flag-ch.svg\" alt=\"France\"></span><span class=\"text-flag\">France</span>\n                {{else}}\n                <span class=\"image-flag\"><img src=\"/product_images/uploaded_images/flag-ch.svg\" alt=\"France\"></span><span class=\"text-flag\">France</span>\n                {{/if}}\n            </a>\n        </li>\n        <li class=\"dropdown-menu-item\">\n            <a href=\"https://ofinto.co.uk\">\n                {{#if is_active}}\n                <span class=\"image-flag\"><img src=\"/product_images/uploaded_images/flag-ch.svg\" alt=\"United Kingdom\"></span><span class=\"text-flag\">United Kingdom</span>\n                {{else}}\n                <span class=\"image-flag\"><img src=\"/product_images/uploaded_images/flag-ch.svg\" alt=\"United Kingdom\"></span><span class=\"text-flag\">United Kingdom</span>\n                {{/if}}\n            </a>\n        </li>\n    </ul>\n</div>"
