{
  "components/themevale/header/header-layout-2": "<header class=\"header themevale-header-layout-2\" role=\"banner\">\r\n    <div class=\"themevale-header-PC\">\r\n        <div class=\"container container-large\">\r\n            <div class=\"themevale_header\">\r\n                <div class=\"header-left\">\r\n                    {{#if page_type '===' 'default'}}\r\n                        <h1 class=\"header-logo\">\r\n                            {{> components/common/store-logo}}\r\n                        </h1>\r\n                    {{else}}\r\n                        <div class=\"header-logo\">\r\n                            {{> components/common/store-logo}}\r\n                        </div>\r\n                    {{/if}}\r\n                    \r\n                </div>\r\n                <div class=\"header-center header-menu\">\r\n                    <div class=\"navPages-container\" id=\"menu\" data-menu>\r\n                        {{> components/common/navigation-menu}}\r\n                    </div>\r\n                </div>\r\n                <div class=\"header-right\">\r\n                    {{> components/common/navigation-2}}\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <div class=\"themevale-header-mobile\">\r\n        {{> components/themevale/header/header-mobile}}\r\n    </div>\r\n</header>\r\n{{{region name=\"home_below_menu\"}}}\r\n\r\n{{#if theme_settings.enable_announcement_bar}}\r\n<div class=\"announcementBar announcementBar-layout-2\">\r\n    <div class=\"container\">\r\n        <div class=\"announcement-bar-list\" data-slick='{\r\n            \"dots\": false,\r\n            \"arrows\": true,\r\n            \"infinite\": true,\r\n            \"mobileFirst\": true,\r\n            \"vertical\": true,\r\n            \"autoplay\": true,\r\n            \"slidesToShow\": 1,\r\n            \"slidesToScroll\": 1,\r\n            \"responsive\": [\r\n            {\r\n                \"breakpoint\": 1024,\r\n                \"settings\": {\r\n                    \"slidesToShow\": 3,\r\n                    \"slidesToScroll\": 3,\r\n                    \"dots\": false,\r\n                    \"arrows\": true,\r\n                    \"vertical\": false\r\n                }\r\n            }            \r\n            ]\r\n        }'>\r\n            {{#if theme_settings.announcement_bar_1_text '!=' ''}}\r\n                <div class=\"announcement-bar-item\">\r\n                    {{#if theme_settings.announcement_bar_1_link '!=' ''}}<a href=\"{{theme_settings.announcement_bar_1_link}}\">{{else}}<span>{{/if}}\r\n                        {{theme_settings.announcement_bar_1_text}}\r\n                    {{#if theme_settings.announcement_bar_1_link '!=' ''}}</a>{{else}}</span>{{/if}}\r\n            </div>\r\n            {{/if}}\r\n            {{#if theme_settings.announcement_bar_2_text '!=' ''}}\r\n                <div class=\"announcement-bar-item\">\r\n                    {{#if theme_settings.announcement_bar_2_link '!=' ''}}<a href=\"{{theme_settings.announcement_bar_2_link}}\">{{else}}<span>{{/if}}\r\n                        {{theme_settings.announcement_bar_2_text}}\r\n                    {{#if theme_settings.announcement_bar_2_link '!=' ''}}</a>{{else}}</span>{{/if}}\r\n                </div>\r\n            {{/if}}    \r\n            {{#if theme_settings.announcement_bar_3_text '!=' ''}}\r\n                <div class=\"announcement-bar-item\">\r\n                    {{#if theme_settings.announcement_bar_3_link '!=' ''}}<a href=\"{{theme_settings.announcement_bar_3_link}}\">{{else}}<span>{{/if}}\r\n                        {{theme_settings.announcement_bar_3_text}}\r\n                    {{#if theme_settings.announcement_bar_3_link '!=' ''}}</a>{{else}}</span>{{/if}}\r\n                </div>\r\n            {{/if}}\r\n        </div>\r\n    </div>\r\n</div>\r\n{{/if}}\r\n\r\n",
  "components/common/navigation-menu": "<nav class=\"navPages\">\n    <ul class=\"navPages-list{{#if theme_settings.navigation_design '!==' 'simple'}} navPages-list-depth-max{{else}} navPages-list-simple{{/if}}\" id=\"navPages-list-main\">\n        {{#each categories}}\n            <li class=\"navPages-item navPages-item-show\">\n            {{#if ../theme_settings.navigation_design '===' 'simple'}}\n                {{> components/common/navigation-list}}\n            {{/if}}\n            {{#if ../theme_settings.navigation_design '===' 'alternate'}}\n                {{> components/common/navigation-list-alternate}}\n            {{/if}}\n            </li>\n        {{/each}}\n        {{#unless theme_settings.hide_content_navigation}}\n            {{#each pages}}\n                {{#if children}}\n                     <li class=\"navPages-item navPages-item-page has-subMenu navPages-item-show\">\n                            <p class=\"navPages-action has-subMenu {{#if name '==' ../page.title}} activePage{{/if}} navPages-action-depth-max is-root\">\n                                <a class=\"text\" href=\"{{url}}\">{{name}}</a> \n                                <span class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">&#10095;</span>\n                            </p>\n                            <div class=\"navPage-subMenu{{#if theme_settings.navigation_design '!==' 'simple'}} navPage-subMenu-horizontal{{/if}} navPage-subMenu-level1\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n                                <div class=\"container\">\n                                    <ul class=\"navPage-subMenu-list navPage-subMenu-list-level1\">\n                                        <li class=\"navPage-subMenu-item-child navPage-subMenu-title\">\n                                            <p class=\"navPage-subMenu-action navPages-action\">\n                                                <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">&#10095;</span>\n                                                <a class=\"text\" href=\"{{url}}\">{{name}}</a>\n                                            </p>\n                                        </li>\n                                        {{#each children}}\n                                            <li class=\"navPage-subMenu-item-child\">\n                                                <a class=\"navPage-subMenu-action navPages-action navPages-action-end\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\n                                            </li>\n                                        {{/each}}\n                                    </ul>\n                                </div>\n                            </div>\n                        </li>\n                    {{else}}\n                        <li class=\"navPages-item navPages-item-page navPages-action-end navPages-item-show\">\n                            <a class=\"navPages-action navPages-action-end {{#if is_active}} activePage{{/if}}\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\n                        </li>\n                    {{/if}}\n             {{/each}}\n        {{/unless}}\n        <li class=\"navPages-item navPages-item-custom u-hiddenVisually has-subMenu\">\n            <p class=\"navPages-action has-subMenu is-root\">\n                <a class=\"text\" href=\"javascript:void(0)\">...</a>\n            </p>\n            <div class=\"navPage-subMenu{{#if theme_settings.navigation_design '!==' 'simple'}} navPage-subMenu-horizontal{{/if}} navPage-subMenu-level1\" id=\"navPages-last\" aria-hidden=\"true\" tabindex=\"-1\">\n                <div class=\"container\">\n                    <ul class=\"navPage-subMenu-list navPage-subMenu-list-level1\">\n                        \n                    </ul>\n                </div>\n            </div>\n        </li>\n    </ul>\n    <ul class=\"navPages-list navPages-list--user\">\n<!--         <li class=\"navPages-item\">\n            <a class=\"navPages-action\" href=\"{{urls.account.wishlists.all}}\">\n                <span class=\"text\"><svg class=\"icon\"><use xlink:href=\"#icon-wishlist\"/></svg>{{lang 'account.mobile_nav.wishlists'}}</span>\n            </a>\n        </li> -->\n        <li class=\"navPages-item\">\n            <a class=\"navPages-action\" href=\"/contact-us/\">\n                <span class=\"text\"><svg class=\"icon\"><use xlink:href=\"#icon-telephone\"/></svg>Kontakt</span>\n            </a>\n        </li>\n        <li class=\"navPages-item\">\n            <a class=\"navPages-action\" href=\"mailto:{{theme_settings.store_owner_email}}\">\n                <span class=\"text\"><svg class=\"icon\"><use xlink:href=\"#icon-envelope\"/></svg>Email: {{theme_settings.store_owner_email}}</span>\n            </a>\n        </li>\n        {{#if settings.gift_certificates_enabled}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action\" href=\"{{urls.gift_certificate.purchase}}\">\n                    <span class=\"text\"><svg class=\"icon\"><use xlink:href=\"#icon-gift\"/></svg>{{lang 'common.gift_cert'}}</span>\n                </a>\n            </li>\n        {{/if}}\n        {{#if currency_selector.currencies.length '>' 1}}\n            <li class=\"navPages-item\">\n                <p class=\"navPages-action navPages-action-currency has-subMenu\" data-collapsible=\"navPages-currency\" aria-controls=\"navPages-currency\" aria-expanded=\"false\">\n                    <a class=\"text\" href=\"javascript:void(0)\">\n                        {{{currency_selector.active_currency_flag}}} {{lang 'common.currency' code=currency_selector.active_currency_code}} \n                    </a>\n                    <span class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">&#10095;</span>\n                </p>\n\n                <div class=\"navPage-subMenu\" id=\"navPages-currency\" aria-hidden=\"true\" tabindex=\"-1\">\n                    <ul class=\"navPage-subMenu-list\">\n                        <li class=\"navPage-subMenu-item-child navPage-subMenu-title\">\n                            <p class=\"navPage-subMenu-action navPages-action\">\n                                <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">&#10095;</span>\n                                <a class=\"text\" href=\"javascript:void(0)\">{{{currency_selector.active_currency_flag}}} {{lang 'common.currency' code=currency_selector.active_currency_code}}</a>\n                            </p>\n                        </li>\n                        {{#each currency_selector.currencies}}\n                        <li class=\"navPage-subMenu-item-child navPages-action-end\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{{switch_url}}}\">\n                                <span class=\"text\">\n                                    {{#if is_active}}\n                                        {{{flag}}} {{name}}\n                                    {{else}}\n                                        {{{flag}}} {{name}}\n                                    {{/if}}\n                                </span>\n                            </a>\n                        </li>\n                        {{/each}}\n                    </ul>\n                </div>\n            </li>\n        {{/if}}\n        \n        {{#if customer.store_credit.value '>' 0}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action navPages-action--storeCredit\">\n                    {{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\n                </a>\n            </li>\n        {{/if}}\n\n<!--        {{#if theme_settings.product_list_display_mode '==' 'list'}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action navPages-action--compare\" href=\"{{urls.compare}}\" data-compare-nav>{{lang 'common.compare'}} <span class=\"countPill countPill--positive countPill--alt\"></span></a>\n            </li>\n        {{/if}}\n-->\n\n        <li class=\"navPages-item\">\n            {{> components/common/social-links}}\n        </li>\n    </ul>\n</nav>\n",
  "components/common/store-logo": "<a href=\"{{urls.home}}\">\n    {{#if settings.store_logo.image}}\n        {{#if theme_settings.logo_size '===' 'original'}}\n            <img class=\"header-logo-image-unknown-size\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n        {{else}}\n            <div class=\"header-logo-image-container\">\n                <img class=\"header-logo-image\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n            </div>\n        {{/if}}\n    {{else}}\n        <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\n    {{/if}}\n</a>\n",
  "components/common/navigation-2": "<nav class=\"navUser\">\r\n    {{> components/common/currency-selector}}\r\n\r\n    <div class=\"navUser-section navUser-section--alt\">\r\n        <div class=\"navUser-item navUser-item--account\">\r\n            <a class=\"navUser-action navUser-action--account\" href=\"javascript:void(0)\">\r\n                {{#if customer}}\r\n                    <span class=\"text\">{{lang 'common.account'}}</span>\r\n                {{else}}\r\n                    <span class=\"text\">{{lang 'common.login'}}</span>\r\n                {{/if}}\r\n         </a>\r\n\r\n            <div class=\"dropdown-menu account-dropdown\" id=\"accountDropdown\">\r\n                {{> components/themevale/login-form}}\r\n            </div>\r\n\r\n        </div>\r\n        <div class=\"navUser-item navUser-item--quichSearch\">\r\n            <a class=\"navUser-action navUser-action--search\" href=\"javascript:void(0)\">\r\n                <svg class=\"icon icon-search\"><use xlink:href=\"#icon-search\"></use></svg>\r\n            </a>\r\n            <div class=\"themevaleSearch\">\r\n                {{> components/common/quick-search}}\r\n            </div>\r\n        </div>\r\n        <div class=\"navUser-item navUser-item--cart\">\r\n            <a\r\n                class=\"navUser-action\"\r\n                data-cart-preview\r\n                data-themevale-dropdown\r\n                data-options=\"align:right\"\r\n                href=\"{{urls.cart}}\">\r\n                <svg class=\"icon\"><use xlink:href=\"#icon-cart\"></use></svg>\r\n                <span class=\"countPill cart-quantity\"></span>\r\n            </a>\r\n        </div>        \r\n    </div>\r\n</nav>\r\n",
  "components/themevale/header/header-mobile": "<div class=\"themevale_header\">\r\n    <div class=\"header-mobile-item text-left\">\r\n        <div class=\"items item--hamburger\">\r\n            <a class=\"mobileMenu-toggle\" href=\"javascript:void(0)\" data-mobile-menu-toggle=\"menu\">\r\n                <span class=\"mobileMenu-toggleIcon\">Toggle menu</span>\r\n            </a>\r\n        </div>\r\n        <div class=\"items item--searchMobile\">\r\n            <a class=\"navUser-action searchMobile\" href=\"javascript:void(0)\" data-search=\"quickSearch\" aria-controls=\"quickSearch\" aria-expanded=\"false\">\r\n                <svg class=\"icon\"><use xlink:href=\"#icon-search\"></use></svg>\r\n                <span class=\"icon close\">&times;</span>\r\n            </a>\r\n        </div>\r\n    </div>\r\n    <div class=\"header-mobile-item text-center\">\r\n        <div class=\"items\">\r\n            <div class=\"logo-wrapper\">\r\n                {{#if page_type '===' 'default'}}\r\n                    <h1 class=\"header-logo\">\r\n                        {{> components/common/store-logo}}\r\n                    </h1>\r\n                {{else}}\r\n                    <div class=\"header-logo\">\r\n                        {{> components/common/store-logo}}\r\n                    </div>\r\n                {{/if}}\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <div class=\"header-mobile-item text-right\">\r\n        <div class=\"items item--account\">\r\n            <a class=\"navUser-action accountMobile\" href=\"javascript:void(0)\">\r\n                <div class=\"navUser-icon\">\r\n                    <svg class=\"icon\"><use xlink:href=\"#icon-user\"/></svg>\r\n                </div>\r\n            </a>\r\n        </div>\r\n        <div class=\"items item--cart themevale_cart\">\r\n            <a class=\"navUser-action cartMobile\"\r\n                data-cart-preview\r\n                data-options=\"align:right\"\r\n                href=\"{{urls.cart}}\">\r\n                <div class=\"navUser-icon navUser-item-cartIcon cart-icon\">\r\n                    <svg class=\"icon\"><use xlink:href=\"#icon-cart\"></use></svg>\r\n                    <span class=\"countPill cart-quantity\"></span>\r\n                </div>\r\n            </a>\r\n        </div>\r\n    </div>\r\n</div>\r\n",
  "components/common/navigation-list-alternate": "{{#if children}}\n    <p class=\"navPages-action navPages-action-depth-max has-subMenu is-root{{#if is_active}} activePage{{/if}}\">\n        <a class=\"text\" href=\"{{url}}\">{{name}}</a> \n        <span class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">&#10095;</span>\n    </p>\n    {{> components/common/navigation-dropdown}}\n{{else}}\n    <a class=\"navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\n{{/if}}\n",
  "components/common/navigation-list": "{{#if children}}\n<p class=\"navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\">\n    <a class=\"text\" href=\"{{url}}\">{{name}}</a> \n    <span class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">&#10095;</span>\n</p>\n<div class=\"navPage-subMenu navPage-subMenu navPage-subMenu-level1\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n    <div class=\"container\">\n        <ul class=\"navPage-subMenu-list navPage-subMenu-list-level1\">\n            <li class=\"navPage-subMenu-item-child navPage-subMenu-title\">\n                <p class=\"navPage-subMenu-action navPages-action\">\n                    <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">&#10095;</span>\n                    <a class=\"text\" href=\"{{url}}\">{{name}}</a>\n                </p>\n            </li>\n            {{#each children}}\n                {{#if children}}\n                    <li class=\"navPage-subMenu-item-child has-subMenu\">\n                        <p class=\"navPage-subMenu-action navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\">\n                            <a class=\"text\" href=\"{{url}}\">{{name}}</a> \n                            <span class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">&#10095;</span>\n                        </p>\n                        <div class=\"navPage-subMenu\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n                            <ul class=\"navPage-subMenu-list\" id=\"navPages-{{id}}\">\n                                <li class=\"navPage-subMenu-item-child navPage-subMenu-title\">\n                                    <p class=\"navPage-subMenu-action navPages-action\">\n                                        <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">&#10095;</span>\n                                        <a class=\"text\" href=\"{{url}}\">{{name}}</a>\n                                    </p>\n                                </li>\n                                {{#each children}}\n                                <li class=\"navPage-subMenu-item-child\">\n                                    <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\n                                </li>\n                                {{/each}}\n                            </ul>\n                        </div>\n                    </li>\n                {{else}}\n                    <li class=\"navPage-subMenu-item-child\">\n                        <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\n                    </li>\n                {{/if}}\n            {{/each}}\n        </ul>\n    </div>\n</div>\n{{else}}\n<a class=\"navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\n{{/if}}\n",
  "components/common/currency-selector": "{{#if currency_selector.currencies.length '>' 1}}\n<div class=\"navUser-section navUser-section--currency\">\n    <div class=\"navUser-item\">\n        <a class=\"navUser-action navUser-action--currencySelector has-dropdown\" href=\"#\" data-dropdown=\"currencySelection\" aria-controls=\"currencySelection\" aria-expanded=\"false\">\n            <span class=\"image-flag\">{{{currency_selector.active_currency_flag}}}</span>\n            <span class=\"text-flag\">{{lang 'common.currency' code=currency_selector.active_currency_code}} </span>\n            <i class=\"icon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i></a>\n        <ul class=\"dropdown-menu\" id=\"currencySelection\" data-dropdown-content aria-hidden=\"true\" tabindex=\"-1\">\n            {{#each currency_selector.currencies}}\n            <li class=\"dropdown-menu-item\">\n                <a href=\"{{{switch_url}}}\">\n                    {{#if is_active}}\n                    {{{flag}}} <strong>{{name}}</strong>\n                    {{else}}\n                    {{{flag}}} {{name}}\n                    {{/if}}\n                </a>\n            </li>\n            {{/each}}\n        </ul>\n    </div>\n</div>\n{{/if}}\n",
  "components/common/social-links": "{{#if theme_settings.enable_social_icon}}\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#if social_media}}\n            {{#each social_media}}\n                <li class=\"socialLinks-item {{display_name}}\">\n                    <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\">\n                        <svg><use xlink:href=\"#icon-{{display_name}}\" /></svg>\n                    </a>\n                </li>\n            {{/each}}\n        {{/if}}\n        {{#if theme_settings.social_icon_google_plus_link '!=' ''}}\n            <li class=\"socialLinks-item google-plus\">\n                <a class=\"icon icon--google-plus\" href=\"{{theme_settings.social_icon_google_plus_link}}\" target=\"_blank\">\n                    {{#if theme_settings.social_icon_google_plus_icon '!=' ''}}\n                        <img data-src=\"https://cdn11.bigcommerce.com/s-{{settings.store_hash}}/product_images/uploaded_images/{{theme_settings.social_icon_google_plus_icon}}\" alt=\"\" />\n                    {{else}}\n                        <svg><use xlink:href=\"#icon-google-plus\" /></svg>\n                    {{/if}}\n                </a>\n            </li>\n        {{/if}}\n\n        {{#if theme_settings.social_icon_snapchat_link '!=' ''}}\n            <li class=\"socialLinks-item snapchat\">\n                <a class=\"icon icon--snapchat\" href=\"{{theme_settings.social_icon_snapchat_link}}\" target=\"_blank\">\n                    {{#if theme_settings.social_icon_snapchat_icon '!=' ''}}\n                        <img data-src=\"https://cdn11.bigcommerce.com/s-{{settings.store_hash}}/product_images/uploaded_images/{{theme_settings.social_icon_snapchat_icon}}\" alt=\"\" />\n                    {{else}}\n                        <svg><use xlink:href=\"#icon-snapchat-ghost\" /></svg>\n                    {{/if}}\n                </a>\n            </li>\n        {{/if}}\n    </ul>\n{{/if}}\n",
  "components/themevale/login-form": "<nav class=\"login-dropdown\" id=\"login-dropdown\">\r\n    {{#if customer}}\r\n        <p class=\"navPage-subMenu-title\">\r\n            <span class=\"text\">Konto</span> \r\n            <span class=\"icon\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></span>\r\n        </p>\r\n        <ul class=\"navPages-list navPages-list--user\">\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.orders.all}}\"><span class=\"text\">{{lang 'account.nav.orders'}}</span></a>\r\n            </li>\r\n            {{#if settings.returns_enabled}}\r\n                <li class=\"navPages-item\">\r\n                    <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.returns}}\"><span class=\"text\">{{lang 'account.nav.returns'}}</span></a>\r\n                </li>\r\n            {{/if}}\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.inbox}}\"><span class=\"text\">{{lang 'account.mobile_nav.messages'}}</span></a>\r\n            </li>\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.addresses}}\"><span class=\"text\">{{lang 'account.nav.addresses'}}</span></a>\r\n            </li>\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.wishlists.all}}\"><span class=\"text\">{{lang 'account.mobile_nav.wishlists'}}</span></a>\r\n            </li>\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.recent_items}}\"><span class=\"text\">{{lang 'account.nav.recently_viewed'}}</span></a>\r\n            </li>\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.details}}\"><span class=\"text\">{{lang 'account.nav.settings'}}</span></a>\r\n            </li>\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.auth.logout}}\"><span class=\"text\">{{lang 'common.logout'}}</span></a>\r\n            </li>\r\n        </ul>\r\n        {{else}}\r\n            <p class=\"navPage-subMenu-title\">\r\n                <span class=\"text\">Kunden Login</span> \r\n                <span class=\"icon\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></span>\r\n            </p>\r\n            <div class=\"popup-login-form\">\r\n                <div class=\"login-content\">\r\n                    <form class=\"login-form form\" action=\"{{settings.secure_base_url}}/login.php?action=check_login\" method=\"post\">\r\n                        {{inject 'useValidEmail' (lang 'forms.validate.common.email_address')}}\r\n                        {{inject 'enterPass' (lang 'forms.validate.common.password')}}\r\n                        <div class=\"form-field\">\r\n\r\n                            <label class=\"form-label\" for=\"login_email\">{{lang 'login.field_email'}} <small>{{lang 'common.required2'}}</small></label>\r\n                            <input class=\"form-input\" name=\"login_email\" id=\"login_email\" type=\"email\" placeholder=\"{{lang 'login.field_email'}}\">\r\n                        </div>\r\n                        <div class=\"form-field\">\r\n                            <label class=\"form-label\" for=\"login_pass\">{{lang 'login.field_password'}} <small>{{lang 'common.required2'}}</small></label>\r\n                            <input class=\"form-input\" id=\"login_pass\" type=\"password\" name=\"login_pass\" placeholder=\"{{lang 'login.field_password'}}\">\r\n                        </div>\r\n                        <div class=\"form-actions\">\r\n                            <input type=\"submit\" class=\"button button--border button--large\" value=\"{{lang 'login.button' }}\">\r\n                            <a class=\"forgot-password\" href=\"{{urls.auth.forgot_password}}\">{{lang 'login.forgot_password' }}</a>\r\n                        </div>\r\n                    </form>\r\n                </div>\r\n                <div class=\"new-customer\">\r\n                    {{#if settings.account_creation_enabled}}\r\n                        <a class=\"button button--primary button--large\" href=\"{{urls.auth.create_account}}\">{{lang 'create_account.button' }}</a>\r\n                    {{/if}}\r\n                </div>\r\n            </div>\r\n        {{/if}}\r\n</nav>\r\n",
  "components/common/quick-search": "<form class=\"form\" action=\"{{urls.search}}\">\n    <fieldset class=\"form-fieldset\">\n        <div class=\"form-field\">\n            <label class=\"is-srOnly\" for=\"search_query\">{{lang \"search.quick_search.input_label\"}}</label>\n            <input type=\"text\" class=\"form-input\" data-search-quick name=\"search_query\" id=\"search_query\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\">\n            <button type=\"submit\"><svg class=\"icon\"><use xlink:href=\"#icon-search\"></use></svg></button>\n        </div>\n    </fieldset>\n</form>\n<div class=\"themevale-dropdown dropdown--quickSearch\" id=\"quickSearch\" aria-hidden=\"true\" tabindex=\"-1\" data-prevent-quick-search-close>\n\t<div class=\"header-search-wrap\">\n        {{#if theme_settings.enable_search_trending}}\n        <div class=\"header-search__trending\">\n        \t{{#if theme_settings.search-trending-title '!==' \"\"}}\n        \t\t<h3 class=\"search-form__product-title\">{{theme_settings.search-trending-title}}</h3>\n        \t{{/if}}\n        \t<ul class=\"list-item\">\n        \t\t{{#if theme_settings.search-trending-text-1 '!==' \"\"}}\n        \t\t\t<li>\n        \t\t\t\t<a href=\"search.php?search_query={{theme_settings.search-trending-text-1}}&section=content\">\n    \t\t    \t\t\t<svg class=\"icon\"><use xlink:href=\"#icon-search\"></use></svg>\n    \t\t    \t\t\t<span>{{theme_settings.search-trending-text-1}}</span>\n        \t\t\t\t</a>\n        \t\t\t</li>\n        \t\t{{/if}}\n        \t\t{{#if theme_settings.search-trending-text-2 '!==' \"\"}}\n        \t\t\t<li>\n        \t\t\t\t<a href=\"search.php?search_query={{theme_settings.search-trending-text-2}}&section=content\">\n    \t    \t\t\t\t<svg class=\"icon\"><use xlink:href=\"#icon-search\"></use></svg>\n    \t    \t\t\t\t<span>{{theme_settings.search-trending-text-2}}</span>\n        \t\t\t\t</a>\n        \t\t\t</li>\n        \t\t{{/if}}\n        \t\t{{#if theme_settings.search-trending-text-3 '!==' \"\"}}\n        \t\t\t<li>\n        \t\t\t\t<a href=\"search.php?search_query={{theme_settings.search-trending-text-3}}&section=content\">\n        \t\t\t\t\t<svg class=\"icon\"><use xlink:href=\"#icon-search\"></use></svg>\n        \t\t\t\t\t<span>{{theme_settings.search-trending-text-3}}</span>\n        \t\t\t\t</a>\n        \t\t\t</li>\n        \t\t{{/if}}\n        \t\t{{#if theme_settings.search-trending-text-4 '!==' \"\"}}\n        \t\t\t<li>\n        \t\t\t\t<a href=\"search.php?search_query={{theme_settings.search-trending-text-4}}&section=content\">\n        \t\t\t\t\t<svg class=\"icon\"><use xlink:href=\"#icon-search\"></use></svg>\n        \t\t\t\t\t<span>{{theme_settings.search-trending-text-4}}</span>\n        \t\t\t\t</a>\n        \t\t\t</li>\n        \t\t{{/if}}\n        \t\t{{#if theme_settings.search-trending-text-5 '!==' \"\"}}\n        \t\t\t<li>\n        \t\t\t\t<a href=\"search.php?search_query={{theme_settings.search-trending-text-5}}&section=content\">\n        \t\t\t\t\t<svg class=\"icon\"><use xlink:href=\"#icon-search\"></use></svg>\n        \t\t\t\t\t<span>{{theme_settings.search-trending-text-5}}</span>\n        \t\t\t\t</a>\n        \t\t\t</li>\n        \t\t{{/if}}\n        \t\t{{#if theme_settings.search-trending-text-6 '!==' \"\"}}\n        \t\t\t<li>\n        \t\t\t\t<a href=\"search.php?search_query={{theme_settings.search-trending-text-6}}&section=content\">\n        \t\t\t\t\t<svg class=\"icon\"><use xlink:href=\"#icon-search\"></use></svg>\n        \t\t\t\t\t<span>{{theme_settings.search-trending-text-6}}</span>\n        \t\t\t\t</a>\n        \t\t\t</li>\n        \t\t{{/if}}\n        \t</ul>\n        </div>\n        {{/if}}\n        \n        {{#if theme_settings.enable_search_product_block}}\n        <div id=\"search_product_block\" class=\"search_product_block\">\n        \t{{#if theme_settings.search-product-block-title '!==' \"\"}}\n        \t\t<h3 class=\"search-form__product-title\">{{theme_settings.search-product-block-title}}</h3>\n        \t{{/if}}\n            <ul class=\"productGrid{{#if theme_settings.show_brand_product_gallery}} show-brand{{/if}}\">\n            </ul>\n        </div>\n        {{/if}}\n    </div>\n    <section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\n</div>\n",
  "components/common/navigation-dropdown": "<div class=\"navPage-subMenu navPage-subMenu-horizontal navPage-subMenu-level1\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n    <ul class=\"navPage-subMenu-list\">\n        <li class=\"navPage-subMenu-item-child navPage-subMenu-title\">\n            <p class=\"navPage-subMenu-action navPages-action\">\n                <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">&#10095;</span>\n                <a class=\"text\" href=\"{{url}}\">{{name}}</a>\n            </p>\n        </li>\n        {{#each children}}\n            {{#if children}}\n                <li class=\"navPage-subMenu-item-child\">\n                    <p class=\"navPage-subMenu-action navPages-action navPages-action-depth-max has-subMenu{{#if is_active}} activePage{{/if}}\">\n                        <a class=\"text\" href=\"{{url}}\">{{name}}</a> \n                        <span class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">&#10095;</span>\n                    </p>\n                    {{> components/common/navigation-dropdown}}\n                </li>\n            {{else}}\n                <li class=\"navPage-subMenu-item-child navPages-action-end\">\n                    <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}} navPages-action-end\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\n                </li>\n            {{/if}}\n        {{/each}}\n    </ul>\n</div>\n"
}
