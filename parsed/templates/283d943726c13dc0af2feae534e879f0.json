  "components/themevale/header/header-layout-2": "\n{{#if theme_settings.enable_announcement_bar}}\n<div class=\"announcementBar announcementBar-layout-2\">\n    <div class=\"container\">\n        <div class=\"announcement-bar-list\" data-slick='{\n            \"dots\": false,\n            \"arrows\": true,\n            \"infinite\": true,\n            \"mobileFirst\": true,\n            \"vertical\": true,\n            \"autoplay\": true,\n            \"slidesToShow\": 1,\n            \"slidesToScroll\": 1,\n            \"responsive\": [\n            {\n                \"breakpoint\": 1024,\n                \"settings\": {\n                    \"slidesToShow\": 3,\n                    \"slidesToScroll\": 3,\n                    \"dots\": false,\n                    \"arrows\": true,\n                    \"vertical\": false\n                }\n            }            \n            ]\n        }'>\n            {{#if theme_settings.announcement_bar_1_text '!=' ''}}\n                <div class=\"announcement-bar-item\">\n                    <svg class=\"icon icon-risk-free-header\"><use xlink:href=\"#icon-risk-free-header\"></use></svg>\n                    {{#if theme_settings.announcement_bar_1_link '!=' ''}}<a href=\"{{theme_settings.announcement_bar_1_link}}\">{{else}}<span>{{/if}}\n                        {{theme_settings.announcement_bar_1_text}}\n                    {{#if theme_settings.announcement_bar_1_link '!=' ''}}</a>{{else}}</span>{{/if}}\n            \t</div>\n            {{/if}}\n            {{#if theme_settings.announcement_bar_2_text '!=' ''}}\n                <div class=\"announcement-bar-item\">\n                    <svg class=\"icon icon-risk-free-header\"><use xlink:href=\"#icon-risk-free-header\"></use></svg>\n                    {{#if theme_settings.announcement_bar_2_link '!=' ''}}<a href=\"{{theme_settings.announcement_bar_2_link}}\">{{else}}<span>{{/if}}\n                        {{theme_settings.announcement_bar_2_text}}\n                    {{#if theme_settings.announcement_bar_2_link '!=' ''}}</a>{{else}}</span>{{/if}}\n                </div>\n            {{/if}}    \n            {{#if theme_settings.announcement_bar_3_text '!=' ''}}\n                <div class=\"announcement-bar-item\">\n                    <svg class=\"icon icon-customer-support\"><use xlink:href=\"#icon-customer-support\"></use></svg>\n                    {{#if theme_settings.announcement_bar_3_link '!=' ''}}<a href=\"{{theme_settings.announcement_bar_3_link}}\">{{else}}<span>{{/if}}\n                        {{{theme_settings.announcement_bar_3_text}}}\n                    {{#if theme_settings.announcement_bar_3_link '!=' ''}}</a>{{else}}</span>{{/if}}\n                </div>\n            {{/if}}\n        </div>\n    </div>\n</div>\n{{/if}}\n<header class=\"header themevale-header-layout-2\" role=\"banner\">\n    \n    \n    {{> components/common/usp-banner-top}}\n    \n    <div class=\"themevale-header-PC\">\n        <div class=\"container\">\n            <div class=\"themevale_header\">\n                <div class=\"header-left\">\n                    {{#if page_type '===' 'default'}}\n                        <h1 class=\"header-logo\">\n                            {{> components/common/store-logo}}\n                        </h1>\n                    {{else}}\n                        <div class=\"header-logo\">\n                            {{> components/common/store-logo}}\n                        </div>\n                    {{/if}}\n                    \n                </div>\n                <div class=\"header-center header-menu\">\n                    <div class=\"navPages-container\" id=\"menu\" data-menu>\n                        {{> components/common/json-mega-menu}}\n                    </div>\n                </div>\n                <div class=\"header-right\">\n                    {{> components/common/navigation-2}}\n                </div>\n            </div>\n        </div>\n    </div>\n    <div class=\"themevale-header-mobile\">\n        {{> components/themevale/header/header-mobile}}\n    </div>\n</header>\n{{{region name=\"home_below_menu\"}}}\n\n\n<script>\n$(document).ready(function() {\n    setTimeout(function(){\n        $('#menu-mobile ul > .nav-item .navUser-action').on('click', function(event) {\n        \tevent.preventDefault();\n        \t$('#menu-mobile .nav-item').removeClass('jsonNav-active');\n        \t$('body').removeClass('jsonNav-active');\n        \t$('#menu-mobile .nav-item .mega-menu-content').hide();\n        \t$(this).closest('#menu-mobile .nav-item').addClass('jsonNav-active');\n        \t$('body').addClass('jsonNav-active');\n        \t$(this).parent().siblings('#menu-mobile .nav-item .mega-menu-content').show();\n        });\n        $('#menu-mobile .mob-hd').on('click', function() {\n    \t\t$('.nav-item').removeClass('jsonNav-active');\n    \t\t$('body').removeClass('jsonNav-active');\n        });\n        $('.mobileMenu-toggle').on('click', function() {\n            $('.nav-item').removeClass('jsonNav-active');\n            $('body').removeClass('jsonNav-active');\n        });\n        \n        $('#menuMobile .navUser-helpMenu').addClass('mega-menu-content');\n    },1500);\n});\n</script>",
