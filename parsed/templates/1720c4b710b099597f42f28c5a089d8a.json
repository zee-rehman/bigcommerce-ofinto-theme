{
  "components/themevale/recently-viewed-products": "{{#if theme_settings.enable_recently_viewed_products }}\n<script type=\"text/javascript\">\n    window.RecentlyViewedProducts({{jsContext}});\n</script>\n<section class=\"recently-viewed-products-sidebar\">\n    <div class=\"lst-seen-widget\">\n        <div class=\"collapse-icon\">\n            <svg class=\"icon\"><use xlink:href=\"#icon-double-arrow-right\"></use></svg>\n        </div>\n        {{#if theme_settings.recently_viewed_products_cart }}\n        <div class=\"top\">\n            <div role=\"link\" class=\"cart-icon with-products\" data-cart-preview>\n                <svg class=\"icon\"><use xlink:href=\"#icon-cart\" /></svg>\n                <span class=\"cartCount cart-badge countPill cart-quantity\"></span>\n            </div>\n        </div>\n        {{/if}}\n        <div class=\"in-content\">\n            {{#if theme_settings.recently_viewed_products_title '!=' ''}}\n                <h3 class=\"title text-center\">{{theme_settings.recently_viewed_products_title}}</h3>\n            {{/if}}\n            <div class=\"products-grid\" id=\"recently-viewed-products-list\">\n                <div class=\"no-products text-center\" style=\"flex-direction: column;display:none;\">\n                    <svg class=\"icon\"><use xlink:href=\"#icon-nopicture\"/></svg>\n                     <span class=\"text\">Es gibt leider keine Produkte.</span>\n                </div>\n            </div>\n            <div id=\"recently-viewed-products-list-tmp\" style=\"display: none !important;\"></div>\n        </div>\n        {{#if theme_settings.enable_back_to_top }}\n            <div class=\"backtoTop bottom\">\n                <span class=\"icon\">&uarr;</span>\n                <span class=\"text\">Hoch</span>\n            </div>\n        {{/if}}\n    </div>\n    \n    <div class=\"wrap-icons\">\n        <div class=\"expand\"><svg class=\"icon\"><use xlink:href=\"#icon-expand\"/></svg></div>\n        {{#if theme_settings.enable_back_to_top }}\n            <div class=\"backtoTop\">\n                <span class=\"icon\">&uarr;</span>\n            </div>\n        {{/if}}\n    </div>\n</section>\n{{/if}}\n"
}