{
  "components/common/navigation-menu": "<nav class=\"navPages\">\n    <ul class=\"navPages-list{{#if theme_settings.navigation_design '!==' 'simple'}} navPages-list-depth-max{{else}} navPages-list-simple{{/if}}\" id=\"navPages-list-main\">\n        {{#each categories}}\n            <li class=\"navPages-item navPages-item-show\">\n            {{#if ../theme_settings.navigation_design '===' 'simple'}}\n                {{> components/common/navigation-list}}\n            {{/if}}\n            {{#if ../theme_settings.navigation_design '===' 'alternate'}}\n                {{> components/common/navigation-list-alternate}}\n            {{/if}}\n            </li>\n        {{/each}}\n        {{#unless theme_settings.hide_content_navigation}}\n            {{#each pages}}\n                {{#if children}}\n                     <li class=\"navPages-item navPages-item-page has-subMenu navPages-item-show\">\n                            <p class=\"navPages-action has-subMenu {{#if name '==' ../page.title}} activePage{{/if}} navPages-action-depth-max is-root\">\n                                <a class=\"text\" href=\"{{url}}\">{{name}}</a> \n                                <span class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">&#10095;</span>\n                            </p>\n                            <div class=\"navPage-subMenu{{#if theme_settings.navigation_design '!==' 'simple'}} navPage-subMenu-horizontal{{/if}} navPage-subMenu-level1\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n                                <div class=\"container\">\n                                    <ul class=\"navPage-subMenu-list navPage-subMenu-list-level1\">\n                                        <li class=\"navPage-subMenu-item-child navPage-subMenu-title\">\n                                            <p class=\"navPage-subMenu-action navPages-action\">\n                                                <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">&#10095;</span>\n                                                <a class=\"text\" href=\"{{url}}\">{{name}}</a>\n                                            </p>\n                                        </li>\n                                        {{#each children}}\n                                            <li class=\"navPage-subMenu-item-child\">\n                                                <a class=\"navPage-subMenu-action navPages-action navPages-action-end\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\n                                            </li>\n                                        {{/each}}\n                                    </ul>\n                                </div>\n                            </div>\n                        </li>\n                    {{else}}\n                        <li class=\"navPages-item navPages-item-page navPages-action-end navPages-item-show\">\n                            <a class=\"navPages-action navPages-action-end {{#if is_active}} activePage{{/if}}\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\n                        </li>\n                    {{/if}}\n             {{/each}}\n        {{/unless}}\n        <li class=\"navPages-item navPages-item-custom u-hiddenVisually has-subMenu\">\n            <p class=\"navPages-action has-subMenu is-root\">\n                <a class=\"text\" href=\"javascript:void(0)\">...</a>\n            </p>\n            <div class=\"navPage-subMenu{{#if theme_settings.navigation_design '!==' 'simple'}} navPage-subMenu-horizontal{{/if}} navPage-subMenu-level1\" id=\"navPages-last\" aria-hidden=\"true\" tabindex=\"-1\">\n                <div class=\"container\">\n                    <ul class=\"navPage-subMenu-list navPage-subMenu-list-level1\">\n                        \n                    </ul>\n                </div>\n            </div>\n        </li>\n    </ul>\n    <ul class=\"navPages-list navPages-list--user\">\n        {{#if settings.show_wishlist}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action\" href=\"{{urls.account.wishlists.all}}\">\n                    <span class=\"text\"><svg class=\"icon\"><use xlink:href=\"#icon-wishlist\"/></svg>{{lang 'account.mobile_nav.wishlists'}}</span>\n                </a>\n            </li>\n        {{/if}}\n        <li class=\"navPages-item\">\n            <a class=\"navPages-action\" href=\"/contact-us\">\n                <span class=\"text\"><svg class=\"icon\"><use xlink:href=\"#icon-telephone\"/></svg>Kontakt</span>\n            </a>\n        </li>\n        <li class=\"navPages-item\">\n            <a class=\"navPages-action\" href=\"mailto:{{theme_settings.store_owner_email}}\">\n                <span class=\"text\"><svg class=\"icon\"><use xlink:href=\"#icon-envelope\"/></svg>Email: {{theme_settings.store_owner_email}}</span>\n            </a>\n        </li>\n        {{#if settings.gift_certificates_enabled}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action\" href=\"{{urls.gift_certificate.purchase}}\">\n                    <span class=\"text\"><svg class=\"icon\"><use xlink:href=\"#icon-gift\"/></svg>{{lang 'common.gift_cert'}}</span>\n                </a>\n            </li>\n        {{/if}}\n\n        <!-- use shop specific file -->\n        {{> shop_specific/navigation-menu }}\n    \n <!--       {{#if customer.store_credit.value '>' 0}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action navPages-action--storeCredit\">\n                    {{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\n                </a>\n            </li>\n        {{/if}}\n-->\n<!--        {{#if theme_settings.product_list_display_mode '==' 'list'}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action navPages-action--compare\" href=\"{{urls.compare}}\" data-compare-nav>{{lang 'common.compare'}} <span class=\"countPill countPill--positive countPill--alt\"></span></a>\n            </li>\n        {{/if}}\n-->\n\n        <li class=\"navPages-item\">\n            {{> components/common/social-links}}\n        </li>\n    </ul>\n</nav>\n",
  "components/common/navigation-list": "{{#if children}}\n<p class=\"navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\">\n    <a class=\"text\" href=\"{{url}}\">{{name}}</a> \n    <span class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">&#10095;</span>\n</p>\n<div class=\"navPage-subMenu navPage-subMenu navPage-subMenu-level1\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n    <div class=\"container\">\n        <ul class=\"navPage-subMenu-list navPage-subMenu-list-level1\">\n            <li class=\"navPage-subMenu-item-child navPage-subMenu-title\">\n                <p class=\"navPage-subMenu-action navPages-action\">\n                    <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">&#10095;</span>\n                    <a class=\"text\" href=\"{{url}}\">{{name}}</a>\n                </p>\n            </li>\n            {{#each children}}\n                {{#if children}}\n                    <li class=\"navPage-subMenu-item-child has-subMenu\">\n                        <p class=\"navPage-subMenu-action navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\">\n                            <a class=\"text\" href=\"{{url}}\">{{name}}</a> \n                            <span class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">&#10095;</span>\n                        </p>\n                        <div class=\"navPage-subMenu\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n                            <ul class=\"navPage-subMenu-list\" id=\"navPages-{{id}}\">\n                                <li class=\"navPage-subMenu-item-child navPage-subMenu-title\">\n                                    <p class=\"navPage-subMenu-action navPages-action\">\n                                        <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">&#10095;</span>\n                                        <a class=\"text\" href=\"{{url}}\">{{name}}</a>\n                                    </p>\n                                </li>\n                                {{#each children}}\n                                <li class=\"navPage-subMenu-item-child\">\n                                    <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\n                                </li>\n                                {{/each}}\n                            </ul>\n                        </div>\n                    </li>\n                {{else}}\n                    <li class=\"navPage-subMenu-item-child\">\n                        <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\n                    </li>\n                {{/if}}\n            {{/each}}\n        </ul>\n    </div>\n</div>\n{{else}}\n<a class=\"navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\n{{/if}}\n",
  "components/common/navigation-list-alternate": "{{#if children}}\n    <p class=\"navPages-action navPages-action-depth-max has-subMenu is-root{{#if is_active}} activePage{{/if}}\">\n        <a class=\"text\" href=\"{{url}}\">{{name}}</a> \n        <span class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">&#10095;</span>\n    </p>\n    {{> components/common/navigation-dropdown}}\n{{else}}\n    <a class=\"navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\n{{/if}}",
  "shop_specific/navigation-menu": "<li class=\"navPages-item\">\n  <p class=\"navPages-action navPages-action-currency has-subMenu\" data-collapsible=\"navPages-currency\" aria-controls=\"navPages-currency\" aria-expanded=\"false\">\n      <a class=\"text\" href=\"javascript:void(0)\">\n          <span class=\"image-flag\"><img src=\"/product_images/uploaded_images/flag-ch.svg\" alt=\"Schweiz\"></span><span>Schweiz</span>\n                  </a>\n      <span class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">&#10095;</span>\n  </p>\n\n  <div class=\"navPage-subMenu\" id=\"navPages-currency\" aria-hidden=\"true\" tabindex=\"-1\">\n      <ul class=\"navPage-subMenu-list\">\n          <li class=\"navPage-subMenu-item-child navPage-subMenu-title\">\n              <p class=\"navPage-subMenu-action navPages-action\">\n                  <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">&#10095;</span>\n                  <a class=\"text\" href=\"javascript:void(0)\"><span class=\"image-flag\"><img src=\"/product_images/uploaded_images/flag-ch.svg\" alt=\"Schweiz\"></span><span class=\"text-flag\">Schweiz</span>\n                  </a>\n              </p>\n          </li>\n          <li class=\"navPage-subMenu-item-child navPages-action-end\">\n              <a class=\"navPage-subMenu-action navPages-action\" href=\"#Weglot-de\">\n                  <span class=\"text\">\n                      {{#if is_active}}\n                          <span class=\"image-flag\"><img src=\"/product_images/uploaded_images/flag-ch.svg\" alt=\"Schweiz (de)\"></span><span class=\"text-flag\">Schweiz (de)</span>\n                      {{else}}\n                          <span class=\"image-flag\"><img src=\"/product_images/uploaded_images/flag-ch.svg\" alt=\"Schweiz (de)\"></span><span class=\"text-flag\">Schweiz (de)</span>\n                      {{/if}}\n                  </span>\n              </a>\n          </li>\n          <li class=\"navPage-subMenu-item-child navPages-action-end\">\n              <a class=\"navPage-subMenu-action navPages-action\" href=\"#Weglot-fr\">\n                  <span class=\"text\">\n                      {{#if is_active}}\n                          <span class=\"image-flag\"><img src=\"/product_images/uploaded_images/flag-fr-ch.svg\" alt=\"Suisse (fr)\"></span><span class=\"text-flag\">Suisse (fr)</span>\n                      {{else}}\n                          <span class=\"image-flag\"><img src=\"/product_images/uploaded_images/flag-fr-ch.svg\" alt=\"Suisse (fr)\"></span><span class=\"text-flag\">Suisse (fr)</span>\n                      {{/if}}\n                  </span>\n              </a>\n          </li>\n          <li class=\"navPage-subMenu-item-child navPages-action-end\">\n              <a class=\"navPage-subMenu-action navPages-action\" href=\"#Weglot-en\">\n                  <span class=\"text\">\n                      {{#if is_active}}\n                          <span class=\"image-flag\"><img src=\"/product_images/uploaded_images/flag-en-ch.svg\" alt=\"Switzerland (en)\"></span><span class=\"text-flag\">Switzerland (en)</span>\n                      {{else}}\n                          <span class=\"image-flag\"><img src=\"/product_images/uploaded_images/flag-en-ch.svg\" alt=\"Switzerland (en)\"></span><span class=\"text-flag\">Switzerland (en)</span>\n                      {{/if}}\n                  </span>\n              </a>\n          </li>\n          <li class=\"navPage-subMenu-item-child navPages-action-end\">\n              <a class=\"navPage-subMenu-action navPages-action\" href=\"https://ofinto.de\">\n                  <span class=\"text\">\n                      {{#if is_active}}\n                          <span class=\"image-flag\"><img src=\"/product_images/uploaded_images/flag-de.svg\" alt=\"Deutschland\"></span><span class=\"text-flag\">Deutschland</span>\n                      {{else}}\n                          <span class=\"image-flag\"><img src=\"/product_images/uploaded_images/flag-de.svg\" alt=\"Deutschland\"></span><span class=\"text-flag\">Deutschland</span>\n                      {{/if}}\n                  </span>\n              </a>\n          </li>\n          <li class=\"navPage-subMenu-item-child navPages-action-end\">\n              <a class=\"navPage-subMenu-action navPages-action\" href=\"https://ofinto.at\">\n                  <span class=\"text\">\n                      {{#if is_active}}\n                          <span class=\"image-flag\"><img src=\"/product_images/uploaded_images/flag-at.svg\" alt=\"Österreich\"></span><span class=\"text-flag\">Österreich</span>\n                      {{else}}\n                          <span class=\"image-flag\"><img src=\"/product_images/uploaded_images/flag-at.svg\" alt=\"Österreich\"></span><span class=\"text-flag\">Österreich</span>\n                      {{/if}}\n                  </span>\n              </a>\n          </li>\n          <li class=\"navPage-subMenu-item-child navPages-action-end\">\n              <a class=\"navPage-subMenu-action navPages-action\" href=\"https://ofinto.fr\">\n                  <span class=\"text\">\n                      {{#if is_active}}\n                          <span class=\"image-flag\"><img src=\"/product_images/uploaded_images/flag-fr.svg\" alt=\"France\"></span><span class=\"text-flag\">France</span>\n                      {{else}}\n                          <span class=\"image-flag\"><img src=\"/product_images/uploaded_images/flag-fr.svg\" alt=\"France\"></span><span class=\"text-flag\">France</span>\n                      {{/if}}\n                  </span>\n              </a>\n          </li>\n      </ul>\n  </div>\n</li>",
  "components/common/social-links": "{{#if theme_settings.enable_social_icon}}\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#if social_media}}\n            {{#each social_media}}\n                <li class=\"socialLinks-item {{display_name}}\">\n                    <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\">\n                        <svg><use xlink:href=\"#icon-{{display_name}}\" /></svg>\n                    </a>\n                </li>\n            {{/each}}\n        {{/if}}\n        {{#if theme_settings.social_icon_google_plus_link '!=' ''}}\n            <li class=\"socialLinks-item google-plus\">\n                <a class=\"icon icon--google-plus\" href=\"{{theme_settings.social_icon_google_plus_link}}\" target=\"_blank\">\n                    {{#if theme_settings.social_icon_google_plus_icon '!=' ''}}\n                        <img data-src=\"https://cdn11.bigcommerce.com/s-{{settings.store_hash}}/product_images/uploaded_images/{{theme_settings.social_icon_google_plus_icon}}\" alt=\"\" />\n                    {{else}}\n                        <svg><use xlink:href=\"#icon-google-plus\" /></svg>\n                    {{/if}}\n                </a>\n            </li>\n        {{/if}}\n\n        {{#if theme_settings.social_icon_snapchat_link '!=' ''}}\n            <li class=\"socialLinks-item snapchat\">\n                <a class=\"icon icon--snapchat\" href=\"{{theme_settings.social_icon_snapchat_link}}\" target=\"_blank\">\n                    {{#if theme_settings.social_icon_snapchat_icon '!=' ''}}\n                        <img data-src=\"https://cdn11.bigcommerce.com/s-{{settings.store_hash}}/product_images/uploaded_images/{{theme_settings.social_icon_snapchat_icon}}\" alt=\"\" />\n                    {{else}}\n                        <svg><use xlink:href=\"#icon-snapchat-ghost\" /></svg>\n                    {{/if}}\n                </a>\n            </li>\n        {{/if}}\n    </ul>\n{{/if}}\n",
  "components/common/navigation-dropdown": "<div class=\"navPage-subMenu navPage-subMenu-horizontal navPage-subMenu-level1\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n    <ul class=\"navPage-subMenu-list\">\n        <li class=\"navPage-subMenu-item-child navPage-subMenu-title\">\n            <p class=\"navPage-subMenu-action navPages-action\">\n                <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">&#10095;</span>\n                <a class=\"text\" href=\"{{url}}\">{{name}}</a>\n            </p>\n        </li>\n        {{#each children}}\n            {{#if children}}\n                <li class=\"navPage-subMenu-item-child\">\n                    <p class=\"navPage-subMenu-action navPages-action navPages-action-depth-max has-subMenu{{#if is_active}} activePage{{/if}}\">\n                        <a class=\"text\" href=\"{{url}}\">{{name}}</a> \n                        <span class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">&#10095;</span>\n                    </p>\n                    {{> components/common/navigation-dropdown}}\n                </li>\n            {{else}}\n                <li class=\"navPage-subMenu-item-child navPages-action-end\">\n                    <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}} navPages-action-end\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\n                </li>\n            {{/if}}\n        {{/each}}\n    </ul>\n</div>\n"
