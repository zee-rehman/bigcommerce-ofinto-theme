{
  "components/products/options/set-radio": "<div class=\"form-field {{toLowerCase this.display_name}}\" data-product-attribute=\"set-radio\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\n        {{ this.display_name }}:\n        {{#if required}}\n            <small>{{lang 'common.required2'}}</small>\n        {{/if}}\n        <span data-option-value></span>\n    </label>\n\n    {{#each this.values}}\n        <input data-filter=\".filter-{{camelcase this.label}}\"\n            class=\"form-radio\"\n            type=\"radio\"\n            id=\"attribute_radio_{{../id}}_{{id}}\"\n            name=\"attribute[{{../id}}]\"\n            value=\"{{id}}\"\n            {{#if selected}}\n                checked\n                data-default\n            {{/if}}\n            {{#if ../required}}required{{/if}}>\n        <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"attribute_radio_{{../id}}_{{id}}\">{{this.label}}</label>\n    {{/each}}\n</div>\n"
}
