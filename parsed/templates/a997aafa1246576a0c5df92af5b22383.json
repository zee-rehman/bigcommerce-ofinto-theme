{
  "components/products/options/set-select": "<div class=\"form-field {{toLowerCase this.display_name}}\" data-product-attribute=\"set-select\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_select_{{id}}\">\n        {{ this.display_name }}:\n        \n        {{#if required}}\n            <small>{{lang 'common.required2'}}</small>\n        {{/if}}\n        <span data-option-value></span>\n    </label>\n\n    <select class=\"form-select form-select--small\" name=\"attribute[{{this.id}}]\" id=\"attribute_select_{{id}}\" {{#if required}}required{{/if}}>\n        <option value=\"\">{{lang 'products.choose_options'}}</option>\n        {{#each this.values}}\n            <option data-filter=\".{{camelcase this.label}}\" data-product-attribute-value=\"{{id}}\" value=\"{{id}}\" {{#if selected}}selected data-default{{/if}}>{{label}}</option>\n        {{/each}}\n    </select>\n</div>\n"
}