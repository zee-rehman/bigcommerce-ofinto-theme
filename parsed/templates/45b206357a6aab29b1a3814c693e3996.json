{
  "components/themevale/themevale_AddOptionForProduct": "<div class=\"productOption\" data-product-id=\"{{product.id}}\">\r\n\t<form class=\"form\" method=\"post\" action=\"{{product.cart_url}}\" enctype=\"multipart/form-data\" data-cart-item-add>\r\n\t\t<input type=\"hidden\" name=\"action\" value=\"add\">\r\n\t\t<input type=\"hidden\" name=\"product_id\" value=\"{{product.id}}\"/>\r\n\t\t<div class=\"productView\">\r\n\t\t    <div data-product-option-change3 style=\"display:none;\">\r\n\t\t        {{#each product.options}}\r\n\t\t        \t{{#if type '===' 'Configurable_PickList_Swatch'}}\r\n\t\t\t\t\t\t{{> components/themevale/themevale-add-option-for-product-swatch url=../../product.url}}\r\n\t\t            {{/if}}\r\n\t\t        {{/each}}\r\n\t\t    </div>\r\n\t\t</div>\r\n\t</form>\r\n</div>\r\n",
  "components/themevale/themevale-add-option-for-product-swatch": "<div class=\"form-field {{toLowerCase this.display_name}}\" data-product-attribute=\"swatch\">\r\n{{#each (limit this.values 4)}}\r\n\t<label class=\"form-option form-option-swatch{{#if pattern}} has-pattern{{/if}}{{#if data.[2]}} three-colors{{/if}}{{#if data.[1]}} two-colors{{/if}}\" for=\"attribute_swatch_{{../id}}_{{id}}_{{../id}}\" data-product-attribute-value=\"{{id}}\">\r\n        {{#if image}}\r\n            <span class='form-option-variant form-option-variant--pattern' title=\"{{this.label}}\" style=\"background-image: url('{{getImage image \"swatch_option_size\"}}');\" data-img-src=\"{{getImage image 'core-swatch'}}\"></span>\r\n        {{/if}}\r\n        {{#if data.[2]}}\r\n            <span class='form-option-variant form-option-variant--color {{this.label}} three-colors' data-color=\"#{{data.[0]}}\" title=\"{{this.label}}\" style=\"background-color: #{{data.[0]}}\"></span>\r\n            <span class='form-option-variant form-option-variant--color {{this.label}} three-colors' data-color-1=\"#{{data.[1]}}\" title=\"{{this.label}}\" style=\"background-color: #{{data.[1]}}\"></span>\r\n            <span class='form-option-variant form-option-variant--color {{this.label}} three-colors' data-color-2=\"#{{data.[2]}}\" title=\"{{this.label}}\" style=\"background-color: #{{data.[2]}}\"></span>\r\n        {{else}}\r\n            {{#if data.[1]}}\r\n                <span class='form-option-variant form-option-variant--color {{this.label}} two-colors' data-color=\"#{{data.[0]}}\" title=\"{{this.label}}\" style=\"background-color: #{{data.[0]}}\"></span>\r\n                <span class='form-option-variant form-option-variant--color {{this.label}} two-colors' data-color-1=\"#{{data.[1]}}\" title=\"{{this.label}}\" style=\"background-color: #{{data.[1]}}\"></span>\r\n            {{else}}\r\n                {{#if data.[0]}}\r\n                    <span class='form-option-variant form-option-variant--color {{this.label}} one-color' data-color=\"#{{data.[0]}}\" title=\"{{this.label}}\" style=\"background-color: #{{data.[0]}}\"></span>\r\n                {{/if}}\r\n            {{/if}}\r\n        {{/if}}\r\n        {{#if pattern}}\r\n            <span class=\"form-option-expanded\">\r\n                <span class=\"form-option-image\" style=\"background-image: url('{{getImage image 'core-swatch'}}');\"></span>\r\n            </span>\r\n        {{/if}}\r\n    </label>\r\n{{/each}}\r\n{{#if this.values.length '>' 4}}\r\n\t<a href=\"{{url}}\" class=\"showmore\">+{{toFixed (subtract this.values.length 4) 0}}</a>\r\n{{/if}}\r\n</div>\n"
}
