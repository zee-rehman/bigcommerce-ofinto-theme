{
  "components/themevale/themevale_AddOptionForProduct": "<div class=\"productOption\" data-product-id=\"{{product.id}}\">\n\t<form class=\"form\" method=\"post\" action=\"{{product.cart_url}}\" enctype=\"multipart/form-data\" data-cart-item-add>\n\t\t<input type=\"hidden\" name=\"action\" value=\"add\">\n\t\t<input type=\"hidden\" name=\"product_id\" value=\"{{product.id}}\"/>\n\t\t<div class=\"productView\">\n\t\t    <div data-product-option-change3 style=\"display:none;\">\n\t\t        {{#each product.options}}\n\t\t        \t{{#if type '===' 'Configurable_PickList_Swatch'}}\n\t\t\t\t\t\t{{> components/themevale/themevale-add-option-for-product-swatch url=../../product.url}}\n\t\t            {{/if}}\n\t\t        {{/each}}\n\t\t    </div>\n\t\t</div>\n\t</form>\n</div>\n",
  "components/themevale/themevale-add-option-for-product-swatch": "<div class=\"form-field {{toLowerCase this.display_name}}\" data-product-attribute=\"swatch\">\n{{#each (limit this.values 4)}}\n\t<label class=\"form-option form-option-swatch{{#if pattern}} has-pattern{{/if}}{{#if data.[2]}} three-colors{{/if}}{{#if data.[1]}} two-colors{{/if}}\" for=\"attribute_swatch_{{../id}}_{{id}}_{{../id}}\" data-product-attribute-value=\"{{id}}\">\n        {{#if image}}\n            <span class='form-option-variant form-option-variant--pattern' title=\"{{this.label}}\" style=\"background-image: url('{{getImage image \"swatch_option_size\"}}');\" data-img-src=\"{{getImage image 'core-swatch'}}\"></span>\n        {{/if}}\n        {{#if data.[2]}}\n            <span class='form-option-variant form-option-variant--color {{this.label}} three-colors' data-color=\"#{{data.[0]}}\" title=\"{{this.label}}\" style=\"background-color: #{{data.[0]}}\"></span>\n            <span class='form-option-variant form-option-variant--color {{this.label}} three-colors' data-color-1=\"#{{data.[1]}}\" title=\"{{this.label}}\" style=\"background-color: #{{data.[1]}}\"></span>\n            <span class='form-option-variant form-option-variant--color {{this.label}} three-colors' data-color-2=\"#{{data.[2]}}\" title=\"{{this.label}}\" style=\"background-color: #{{data.[2]}}\"></span>\n        {{else}}\n            {{#if data.[1]}}\n                <span class='form-option-variant form-option-variant--color {{this.label}} two-colors' data-color=\"#{{data.[0]}}\" title=\"{{this.label}}\" style=\"background-color: #{{data.[0]}}\"></span>\n                <span class='form-option-variant form-option-variant--color {{this.label}} two-colors' data-color-1=\"#{{data.[1]}}\" title=\"{{this.label}}\" style=\"background-color: #{{data.[1]}}\"></span>\n            {{else}}\n                {{#if data.[0]}}\n                    <span class='form-option-variant form-option-variant--color {{this.label}} one-color' data-color=\"#{{data.[0]}}\" title=\"{{this.label}}\" style=\"background-color: #{{data.[0]}}\"></span>\n                {{/if}}\n            {{/if}}\n        {{/if}}\n        {{#if pattern}}\n            <span class=\"form-option-expanded\">\n                <span class=\"form-option-image\" style=\"background-image: url('{{getImage image 'core-swatch'}}');\"></span>\n            </span>\n        {{/if}}\n    </label>\n{{/each}}\n{{#if this.values.length '>' 4}}\n\t<a href=\"{{url}}\" class=\"showmore\">+{{toFixed (subtract this.values.length 4) 0}}</a>\n{{/if}}\n</div>\n"
}