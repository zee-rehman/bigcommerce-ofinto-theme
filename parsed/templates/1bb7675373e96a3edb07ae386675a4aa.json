{
  "components/amp/common/navigation-menu": "<nav class=\"navPages\">\n    <ul class=\"navPages-list\">\n        {{#each categories}}\n            <li class=\"navPages-item\">\n                {{> components/common/navigation-list}}\n            </li>\n        {{/each}}\n        {{#unless theme_settings.hide_content_navigation}}\n            {{#each pages}}\n                 <li class=\"navPages-item\">\n                     <a class=\"navPages-action\" href=\"{{url}}\">{{name}}</a>\n                 </li>\n             {{/each}}\n        {{/unless}}\n    </ul>\n    <ul class=\"navPages-list navPages-list--user\">\n        {{#if currency_selector.currencies.length '>' 1}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action has-subMenu\" href=\"#\" data-collapsible=\"navPages-currency\" aria-controls=\"navPages-currency\" aria-expanded=\"false\">\n                    Schweiz <span class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-chevron-down\"></use></svg></span>\n                </a>\n                <div class=\"navPage-subMenu\" id=\"navPages-currency\" aria-hidden=\"true\" tabindex=\"-1\">\n                    <ul class=\"navPage-subMenu-list\">\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"https://ofinto.ch\">\n                                {{#if is_active}}\n                                    Schweiz (de)\n                                {{else}}\n                                    Schweiz (de)\n                                {{/if}}\n                            </a>\n                        </li>\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"https://fr.ofinto.ch\">\n                                {{#if is_active}}\n                                    Suisse (fr)\n                                {{else}}\n                                    Suisse (fr)\n                                {{/if}}\n                            </a>\n                        </li>\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"https://en.ofinto.ch\">\n                                {{#if is_active}}\n                                    Switzerland (en)\n                                {{else}}\n                                    Switzerland (en)\n                                {{/if}}\n                            </a>\n                        </li>\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"https://ofinto.de\">\n                                {{#if is_active}}\n                                    Deutschland\n                                {{else}}\n                                    Deutschland\n                                {{/if}}\n                            </a>\n                        </li>\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"https://ofinto.fr\">\n                                {{#if is_active}}\n                                    France\n                                {{else}}\n                                    France\n                                {{/if}}\n                            </a>\n                        </li>\n                    </ul>\n                </div>\n            </li>\n        {{/if}}\n        <li class=\"navPages-item\">\n            <a class=\"navPages-action\" href=\"{{urls.gift_certificate.purchase}}\">{{lang 'common.gift_cert'}}</a>\n        </li>\n        {{#if customer.store_credit.value '>' 0}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action navPages-action--storeCredit\">\n                    {{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\n                </a>\n            </li>\n        {{/if}}\n        {{#if theme_settings.product_list_display_mode '==' 'list'}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action navPages-action--compare\" href=\"{{urls.compare}}\" data-compare-nav>{{lang 'common.compare'}} <span class=\"countPill countPill--positive countPill--alt\"></span></a>\n            </li>\n        {{/if}}\n        {{#if customer}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action has-subMenu\" href=\"{{urls.account.index}}\" data-collapsible=\"navPages-account\">\n                    {{lang 'common.account'}} <span class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></span>\n                </a>\n                <div class=\"navPage-subMenu\" id=\"navPages-account\" aria-hidden=\"true\" tabindex=\"-1\">\n                    <ul class=\"navPage-subMenu-list\">\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.orders.all}}\">{{lang 'account.nav.orders'}}</a>\n                        </li>\n                        {{#if settings.returns_enabled}}\n                            <li class=\"navPage-subMenu-item\">\n                                <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.returns}}\">{{lang 'account.nav.returns'}}</a>\n                            </li>\n                        {{/if}}\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.addresses}}\">{{lang 'account.nav.addresses'}}</a>\n                        </li>\n                        {{#if settings.show_payment_methods}}\n                            <li class=\"navPage-subMenu-item\">\n                                <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.payment_methods.all}}\">{{lang 'account.nav.payment_methods'}}</a>\n                            </li>\n                        {{/if}}\n                        {{#if settings.show_wishlist}}\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.wishlists.all}}\">{{lang 'account.mobile_nav.wishlists'}}</a>\n                        </li>\n                        {{/if}}\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.recent_items}}\">{{lang 'account.nav.recently_viewed'}}</a>\n                        </li>\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.details}}\">{{lang 'account.nav.settings'}}</a>\n                        </li>\n                    </ul>\n                </div>\n            </li>\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action\" href=\"{{urls.auth.logout}}\">{{lang 'common.logout'}}</a>\n            </li>\n        {{else}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action\" href=\"{{urls.auth.login}}\">{{lang 'common.login'}}</a> {{lang 'common.or'}} <a class=\"navPages-action\" href=\"{{urls.auth.create_account}}\">{{lang 'common.sign_up'}}</a>\n            </li>\n        {{/if}}\n    </ul>\n</nav>\n",
  "components/common/navigation-list": "{{#if children}}\n<p class=\"navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\">\n    <a class=\"text\" href=\"{{url}}\">{{name}}</a> \n    <span class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">&#10095;</span>\n</p>\n<div class=\"navPage-subMenu navPage-subMenu navPage-subMenu-level1\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n    <div class=\"container\">\n        <ul class=\"navPage-subMenu-list navPage-subMenu-list-level1\">\n            <li class=\"navPage-subMenu-item-child navPage-subMenu-title\">\n                <p class=\"navPage-subMenu-action navPages-action\">\n                    <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">&#10095;</span>\n                    <a class=\"text\" href=\"{{url}}\">{{name}}</a>\n                </p>\n            </li>\n            {{#each children}}\n                {{#if children}}\n                    <li class=\"navPage-subMenu-item-child has-subMenu\">\n                        <p class=\"navPage-subMenu-action navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\">\n                            <a class=\"text\" href=\"{{url}}\">{{name}}</a> \n                            <span class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">&#10095;</span>\n                        </p>\n                        <div class=\"navPage-subMenu\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n                            <ul class=\"navPage-subMenu-list\" id=\"navPages-{{id}}\">\n                                <li class=\"navPage-subMenu-item-child navPage-subMenu-title\">\n                                    <p class=\"navPage-subMenu-action navPages-action\">\n                                        <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">&#10095;</span>\n                                        <a class=\"text\" href=\"{{url}}\">{{name}}</a>\n                                    </p>\n                                </li>\n                                {{#each children}}\n                                <li class=\"navPage-subMenu-item-child\">\n                                    <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\n                                </li>\n                                {{/each}}\n                            </ul>\n                        </div>\n                    </li>\n                {{else}}\n                    <li class=\"navPage-subMenu-item-child\">\n                        <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\n                    </li>\n                {{/if}}\n            {{/each}}\n        </ul>\n    </div>\n</div>\n{{else}}\n<a class=\"navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\n{{/if}}\n"
