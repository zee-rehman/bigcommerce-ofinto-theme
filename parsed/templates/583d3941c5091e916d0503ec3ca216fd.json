{
  "components/products/description-tabs": "<ul class=\"tabs tab-horizontal\" data-tab>\n    <li class=\"tab is-active\">\n        <a class=\"tab-title\" href=\"#tab-description\">{{lang 'products.description'}}</a>\n    </li>\n    {{#if theme_settings.show_shipping_tab}}\n        <li class=\"tab\">\n          <a class=\"tab-title\" href=\"#tab-shipping\">{{#if theme_settings.shipping_tab_title '!=' ''}}{{theme_settings.shipping_tab_title}} {{else}}{{lang 'products.shipping_tab'}}{{/if}}</a>\n        </li>\n    {{/if}}\n    {{#all settings.show_product_reviews theme_settings.show_product_reviews_tab}}\n        <li class=\"tab\">\n          <a class=\"tab-title productView-reviewTabLink\" href=\"#tab-reviews\">{{lang 'products.reviews_tab'}}</a>\n        </li>\n    {{/all}} Judge.me change\n    {{#if product.warranty}}\n        <li class=\"tab\">\n            <a class=\"tab-title\" href=\"#tab-warranty\">{{lang 'products.warranty'}}</a>\n        </li>\n    {{/if}}\n    {{#if theme_settings.show_custom_tab}}\n        <li class=\"tab custom_tab\">\n          <a class=\"tab-title\" href=\"#tab-custom\">{{#if theme_settings.custom_tab_title '!=' ''}}{{theme_settings.custom_tab_title}}{{else}}{{lang 'products.custom_tab'}}{{/if}}</a>\n        </li>\n    {{/if}}\n</ul>\n<div class=\"tabs-contents tab-horizontal\">\n    <div class=\"tab-content is-active\" id=\"tab-description\">\n      <div class=\"toggle-title\">\n          <a class=\"toggleLink\" href=\"#tab-description-content\" data-collapsible>\n              <span class=\"title\">{{lang 'products.description'}}</span>\n              <span class=\"icon\"><svg><use xlink:href=\"#icon-chevron-down\"></use></svg></span>\n          </a>\n      </div>\n      <div class=\"tab-content-inner is-open\" id=\"tab-description-content\">\n        {{{product.description}}}\n        {{#if theme_settings.enable_ask_an_expert}}\n            <div class=\"themevale_ask\">\n              <svg class=\"icon\"><use xlink:href=\"#icon-ask\"></use></svg>\n              {{#if theme_settings.enable_ask_an_expert}}\n                <span class=\"text\">{{theme_settings.ask_an_expert_text}}</span>\n              {{/if}}  \n                <a href=\"#\" data-reveal-id=\"ask-an-expert\">{{theme_settings.ask_an_expert_subject}}</a>\n            </div>\n        {{/if}}\n        \n      </div>\n    </div>\n\n    {{#if theme_settings.show_shipping_tab}}\n      <div class=\"tab-content\" id=\"tab-shipping\">\n        <div class=\"toggle-title\">\n          <a class=\"toggleLink\" href=\"#tab-shipping-content\" data-collapsible>\n              <span class=\"title\">{{#if theme_settings.shipping_tab_title '!=' ''}}{{theme_settings.shipping_tab_title}} {{else}}{{lang 'products.shipping_tab'}}{{/if}}</span>\n              <span class=\"icon\"><svg><use xlink:href=\"#icon-chevron-down\"></use></svg></span>\n          </a>\n        </div>\n        <div class=\"tab-content-inner\" id=\"tab-shipping-content\"></div>\n      </div>\n    {{/if}}\n\n    <!-- {{#all settings.show_product_reviews theme_settings.show_product_reviews_tab}} Judge.me change -->\n    {{#if theme_settings.show_product_reviews_tab}} <!-- Judge.me change -->\n      <div class=\"tab-content\" id=\"tab-reviews\">\n        <div class=\"toggle-title\">\n          <a class=\"toggleLink\" href=\"#tab-reviews-content\" data-collapsible>\n              <span class=\"title\">{{lang 'products.reviews_tab'}}</span>\n              <span class=\"icon\"><svg><use xlink:href=\"#icon-chevron-down\"></use></svg></span>\n          </a>\n        </div>\n        <div class=\"tab-content-inner\" id=\"tab-reviews-content\">\n          {{> components/products/reviews reviews=product.reviews product=product urls=urls}}\n        </div>\n      </div>\n    {{/if}} <!-- Judge.me change -->\n    <!-- {{/all}} Judge.me change -->\n\n   {{#if product.warranty}}\n      <div class=\"tab-content\" id=\"tab-warranty\">\n        <div class=\"toggle-title\">\n          <a class=\"toggleLink\" href=\"#tab-warranty-content\" data-collapsible>\n              <span class=\"title\">{{lang 'products.warranty'}}</span>\n              <span class=\"icon\"><svg><use xlink:href=\"#icon-chevron-down\"></use></svg></span>\n          </a>\n        </div>\n        <div class=\"tab-content-inner\" id=\"tab-warranty-content\">\n          {{{product.warranty}}}\n        </div>\n      </div>\n   {{/if}}\n\n   {{#if theme_settings.show_custom_tab}}\n      <div class=\"tab-content\" id=\"tab-custom\">\n        <div class=\"toggle-title\">\n          <a class=\"toggleLink\" href=\"#tab-custom-content\" data-collapsible>\n              <span class=\"title\">{{#if theme_settings.custom_tab_title '!=' ''}}{{theme_settings.custom_tab_title}}{{else}}{{lang 'products.custom_tab'}}{{/if}}</span>\n              <span class=\"icon\"><svg><use xlink:href=\"#icon-chevron-down\"></use></svg></span>\n          </a>\n        </div>\n        <div class=\"tab-content-inner\" id=\"tab-custom-content\"></div>\n      </div>\n    {{/if}}\n</div>\n",
  "components/products/reviews": "<section class=\"toggle productReviews\" id=\"product-reviews\" data-product-reviews>\n    <!-- Judge.me Reviews -->\n    <div id='judgeme_product_reviews' class='jdgm-widget jdgm-review-widget' data-product-title='{{product.title}}'\n        data-id='{{product.id}}'></div>\n    <!-- Judge.me Reviews End -->\n<!--     <h4 class=\"toggle-title\">\n        {{lang 'products.reviews.header' total=reviews.total}}\n        {{#if theme_settings.show_product_reviews_tabs '!==' true}}\n            <a class=\"toggleLink is-open\" data-collapsible href=\"#productReviews-content\">\n                <span class=\"toggleLink-text toggleLink-text--on\">\n                    {{lang 'products.reviews.hide'}}\n                </span>\n                <span class=\"toggleLink-text toggleLink-text--off\">\n                    {{lang 'products.reviews.show'}}\n                </span>\n            </a>\n        {{/if}}\n    </h4>\n    <div class=\"toggle-content is-open\" id=\"productReviews-content\" aria-hidden=\"false\">\n        {{#if reviews.total}}\n        <ul class=\"productReviews-list\" id=\"productReviews-list\">\n            {{#each reviews.list}}\n            <li class=\"productReview\">\n                <article itemprop=\"review\" itemscope itemtype=\"http://schema.org/Review\">\n                    <header>\n                        <div class=\"productReview-title-group\">\n                            <h5 itemprop=\"name\" class=\"productReview-title\">{{ title }}</h5>\n                            <span class=\"productReview-rating rating--small\" itemprop=\"reviewRating\" itemscope itemtype=\"http://schema.org/Rating\">\n                                {{> components/products/ratings rating=rating}}\n                                <span class=\"productReview-ratingNumber\" itemprop=\"ratingValue\">{{ rating }}</span>\n                            </span>\n                        </div>\n                        {{#if name}}\n                            <meta itemprop=\"author\" content=\"{{name}}\">\n                            <p class=\"productReview-author\">\n                                {{lang 'products.reviews.post_on_by' date=date name=name }}\n                            </p>\n                        {{else}}\n                            <p class=\"productReview-author\">\n                                {{lang 'products.reviews.post_on_by' date=date name=(lang 'products.reviews.anonymous_poster')}}\n                            </p>\n                        {{/if}}\n                    </header>\n                    <p itemprop=\"reviewBody\" class=\"productReview-body\">{{{ sanitize text }}}</p>\n                </article>\n            </li>\n            {{/each}}\n        </ul>\n        <div class=\"btn-group\">\n            {{> components/common/paginator pagination.reviews}}\n            <div class=\"btn-createReview\">\n                <a class=\"button button--small\" href=\"#\" data-reveal-id=\"modal-review-form\">{{lang 'products.reviews.write_a_review'}}</a>\n            </div>\n        </div>\n        {{else}}\n            <div class=\"alert alert-warning\">\n                Dieses Produkt wurde noch nicht bewertet. Sei der erste, der eine Bewertung abgibt!\n            </div>\n            <div class=\"btn-createReview\">\n                <a class=\"button button--small\" href=\"#\" data-reveal-id=\"modal-review-form\">{{lang 'products.reviews.write_a_review'}}</a>\n            </div>\n        {{/if}}\n    </div>\n --></section>\n",
  "components/common/paginator": "<div class=\"pagination\" data-products=\"{{number_products}}\" data-total=\"{{total}}\"> \n    <ul class=\"pagination-list\">\n        {{#if previous}}\n            <li class=\"pagination-item pagination-item--previous\">\n                <a class=\"pagination-link\" href=\"{{previous}}\" {{#unless reload}}data-faceted-search-facet{{/unless}}>\n                    <i class=\"icon\" aria-hidden=\"true\">\n                        <svg>\n                            <use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-chevron-left\"></use>\n                        </svg>\n                    </i>\n                    {{lang 'common.previous'}}\n                </a>\n            </li>\n        {{/if}}\n        {{#each links}}\n            {{#if this.number '==' ../current}}\n                <li class=\"pagination-item pagination-item--current\">\n                    <a class=\"pagination-link\" href=\"{{this.url}}\" {{#unless reload}}data-faceted-search-facet{{/unless}}>{{this.number}}</a>\n                </li>\n            {{else}}\n                <li class=\"pagination-item\">\n                    <a class=\"pagination-link\" href=\"{{this.url}}\" {{#unless reload}}data-faceted-search-facet{{/unless}}>{{this.number}}</a>\n                </li>\n            {{/if}}\n        {{/each}}\n\n        {{#if next}}\n            <li class=\"pagination-item pagination-item--next\">\n                <a class=\"pagination-link\" href=\"{{next}}\" {{#unless reload}}data-faceted-search-facet{{/unless}}>\n                    {{lang 'common.next'}}\n                    <i class=\"icon\" aria-hidden=\"true\">\n                        <svg>\n                            <use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-chevron-right\"></use>\n                        </svg>\n                    </i>\n                </a>\n            </li>\n        {{/if}}\n    </ul>\n</div>\n",
  "components/products/ratings": "{{#for 1 5}}\n    {{#if ../this.rating '>=' $index}}\n        <span class=\"icon icon--ratingFull\">\n            <svg>\n                <use xlink:href=\"#icon-star\" />\n            </svg>\n        </span>\n    {{else}}\n        <span class=\"icon icon--ratingEmpty\">\n            <svg>\n                <use xlink:href=\"#icon-star\" />\n            </svg>\n        </span>\n    {{/if}}\n{{/for}}\n"
}
