  "components/common/navigation-2": "<nav class=\"navUser\">\n    \n    <div class=\"navUser-section navUser-section--help\">\n        {{!-- Help header nav and dropdown appear dynamically from json customize file --}}\n    </div>\n    \n    {{> components/common/currency-selector}}\n\n    <div class=\"navUser-section navUser-section--alt\">\n        <div class=\"navUser-item navUser-item--quichSearch\">\n            <a class=\"navUser-action navUser-action--search\" href=\"javascript:void(0)\">\n                <svg class=\"icon icon-search\"><use xlink:href=\"#icon-search\"></use></svg>\n            </a>\n            <div class=\"themevaleSearch\">\n                {{> components/common/quick-search}}\n            </div>\n        </div>\n        \n        <div class=\"navUser-item navUser-item--account\">\n            <a class=\"navUser-action navUser-action--account\" href=\"javascript:void(0)\">\n                {{#if customer}}\n                    <span class=\"text\">\n                        <svg class=\"icon icon-user-header\"><use xlink:href=\"#icon-user\"></use></svg>\n                    </span>\n                {{else}}\n                    <span class=\"text\">\n                        <svg class=\"icon icon-user-header\"><use xlink:href=\"#icon-user\"></use></svg>\n                    </span>\n                {{/if}}\n            </a>\n\n            <div class=\"dropdown-menu account-dropdown\" id=\"accountDropdown\">\n                {{> components/themevale/login-form}}\n            </div>\n\n        </div>\n        \n        <div class=\"navUser-item navUser-item--cart\">\n            <a\n                class=\"navUser-action\"\n                data-cart-preview\n                data-themevale-dropdown\n                data-options=\"align:right\"\n                href=\"{{urls.cart}}\">\n                <svg class=\"icon\"><use xlink:href=\"#icon-cart\"></use></svg>\n                <span class=\"countPill cart-quantity\"></span>\n            </a>\n        </div>        \n    </div>\n</nav>",
