{
  "components/products/videos": "<div id=\"video-gallery-popup\" class=\"modal\" data-reveal>\n    <div class=\"modal-content\">\n        <a href=\"#\" class=\"modal-close\" aria-label=\"Close\">\n            <svg class=\"icon\"><use xlink:href=\"#icon-close\"></use></svg>\n        </a>\n        <div class=\"modal-header\">\n            <h2 class=\"modal-header-title\">Video Gallery</h2>\n        </div>\n        <div class=\"modal-body\">\n            <div class=\"videoGallery themevale_videoGallery\" data-video-gallery>\n                <div id=\"videoGallery-content\">\n                    <div class=\"videoGallery-main\">\n                        <iframe\n                            id=\"player\"\n                            class=\"lazyload\"\n                            type=\"text/html\"\n                            width=\"640\"\n                            height=\"390\"\n                            frameborder=\"0\"\n                            webkitAllowFullScreen\n                            mozallowfullscreen\n                            allowFullScreen\n                            data-src=\"//www.youtube.com/embed/{{this.featured.id}}?rel=0\"\n                            data-video-player>\n                        </iframe>\n                    </div>\n                    <ul class=\"videoGallery-list scrollbar\">\n                        {{#each this.list}}\n                            <li class=\"videoGallery-item\">\n                                <a href=\"#\" class=\"video {{#if @first}}is-active{{/if}}\" data-video-item data-video-id=\"{{id}}\">\n                                    <div class=\"video-figure\">\n                                        <img class=\"lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"//i.ytimg.com/vi/{{id}}/default.jpg\"/>\n                                    </div>\n                                    <div class=\"video-body\">\n                                        <h5 class=\"video-title\">{{title_long}}</h5>\n                                        <p class=\"video-description\">{{description_short}}</p>\n                                    </div>\n                                </a>\n                            </li>\n                        {{/each}}\n                    </ul>\n                </div>\n            </div>\n            <div class=\"loadingOverlay\"></div>\n        </div>\n    </div>\n</div>\n"
}
